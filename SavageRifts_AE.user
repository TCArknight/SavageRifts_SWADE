      </bootstrap>
    </thing>
  <thing id="faSRADemonSlayers" name="Demon Slayers" description="Momano begin with Occult d8 linked with the Scholar Edge, plus the Monster Slayer Edge." compset="FraAbility">
    <usesource source="srcRiftsEmpHumAE"/>
    <bootstrap thing="skOccult"></bootstrap>
    <bootstrap thing="edgScholarSWADE">
      <autotag group="thing" tag="skipprereq"/>
      <assignval field="usrChosen1" value="skOccult" behavior="pick"/>
      </bootstrap>
    <bootstrap thing="edgSRAMonstSlayer"></bootstrap>
    <eval phase="PreTraits" priority="5000">~Occult d8.
#traitcreation[skOccult] += 2
hero.childfound[resSkill].field[resSpent].value -= 1</eval>
    </thing>
  <thing id="faSRAHarmonyBody" name="Harmony of the Body" description="Momano start play with Athletics d6, Fighting d6, and the Chi Edge." compset="FraAbility">
