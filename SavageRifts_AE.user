  <thing id="rpSRADBeeMajQuF" name="D-Bee (Major)" description="The initial Reactions to Quick Flex typically start at Unfriendly, or Hostile for human supremacists. Those with an Unfriendly or lower Reaction will refuse to sell wares or provide services such as healing, repairs, etc. Failed social checks with Hostile parties often result in violence." compset="RacialProp" summary="Initial reactions begin at Unfriendly (or Hostile for Human supremacists.">
    <usesource source="srcRiftsTLPGAE"/>
    </thing>
  <thing id="rpSRAElusiveQuF" name="Elusive" description="Quick-Flex are difficult to pin down in close quarters, giving them a natural +1 to their Parry." compset="RacialProp" summary="+1 to Parry.">
    <usesource source="srcRiftsTLPGAE"/>
    <eval phase="PreTraits" priority="5000"><![CDATA[perform #traitadjust[trParry,+,1,"Elusive"]]]></eval>
    </thing>
  <thing id="rpSRAFastQuF" name="Fast" description="Quick-Flex have +2 t o Pace and increase their Running die by one die type (usually a Pace of 8 and d8 Running die)." compset="RacialProp" summary="Pace +2, Increase Running die by one die type.">
    <usesource source="srcRiftsTLPGAE"/>
    <eval phase="Traits" priority="5500"><![CDATA[perform #traitadjust[trPace,+,2,"Fast"]]]>
      <before name="Derived trtFinal"/>
      <after name="Set Pace"/>
      </eval>
    <eval phase="PreTraits" priority="6000" index="2"><![CDATA[      	if (hero.tagis[source.RunningDie] <> 0) then
          perform #traitadjust[trRunning,+,2,"Fast"]
          endif]]></eval>
    </thing>
  <thing id="rpSRAQuickQuF" name="Quick" description="Begin play with the Quick Edge." compset="RacialProp">
    <usesource source="srcRiftsTLPGAE"/>
    <tag group="Hide" tag="Special"/>
    <bootstrap thing="edgQuick">
      <autotag group="thing" tag="skipprereq"/>
      </bootstrap>
    </thing>
  <thing id="rpSRAResPathQuF" name="Restricted Path" description="Quick Flex cannot take any Arcane Background using PPE, nor any Iconic Framework that includes it." compset="RacialProp" summary="Cannot take any Arcane Background using PPE, nor any Iconic Framework that includes it.">
    <usesource source="srcRiftsTLPGAE"/>
    <eval phase="Effects" priority="5000">perform hero.assign[HelperRiftsAE.PPENotValid]</eval>
    </thing>
  <thing id="rpSRAShortAttQuF" name="Short Attention Span" description="Quick-Flex have a difficult time sitting still and focusing on intellectual pursuits. They suffer a −1 to all Smarts rolls, but not Smarts linked skill checks." compset="RacialProp" summary="−1 to all Smarts rolls (but not Smarts linked skill checks)." uniqueness="unique">
    <usesource source="srcRiftsTLPGAE"/>
    </thing>
  <thing id="rpSRASupConQuF" name="Supreme Confidence" description="All Quick Flex are inherently Overconfident in addition to any other Hindrances chosen by the player." compset="RacialProp">
    <usesource source="srcRiftsTLPGAE"/>
    <tag group="Hide" tag="Special"/>
    <bootstrap thing="hinConfide"></bootstrap>
    </thing>
  <thing id="rpSRAAniEmpSim" name="Animal Empathy" description="Simvan’s psionic connection to animals grants the Beast Master Edge and Riding d4." compset="RacialProp" summary="Begin with Riding d4.">
    <usesource source="srcRiftsTLPGAE"/>
    <bootstrap thing="skRiding"></bootstrap>
    <bootstrap thing="edgBeastMaster"></bootstrap>
    </thing>
  <thing id="rpSRABadRepMajSim" name="Bad Reputation (Major)" description="Simvan are despised by folks due to their penchant for cannibalism, when dealing with most people their initial Reactions are two levels worse than usual." compset="RacialProp" summary="Initial Reactions are two levels worse than usual.">
    <usesource source="srcRiftsTLPGAE"/>
    </thing>
  <thing id="rpSRABloodthirSim" name="Bloodthirsty" description="For Simvan, the ultimate victory is defeating and consuming a worthy opponent, a conception which leads to a level of callous violence uncommon even on Rifts Earth." compset="RacialProp" summary="Ultimate victory is defeating and consuming a worthy opponent.">
    <usesource source="srcRiftsTLPGAE"/>
    </thing>
  <thing id="rpSRADBeeMajSim" name="D-Bee (Major)" description="Simvan tribes prey on humans, making even independent Simvan misunderstood by others. Initial Reactions typically start Hostile at best, and human supremacists are immediately violent unless a social skill check is successful. Those with a Unfriendly or lower Reactions will refuse to sell wares or provide services such as healing, repairs, etc." compset="RacialProp" summary="Initial reactions begin at Hostile (or immediate violence for Human supremacists.">
    <usesource source="srcRiftsTLPGAE"/>
    </thing>
  <thing id="rpSRAInstinctSim" name="Instinct over Intellect" description="Simvan act on their instincts, intellectual pursuits are not common for them. They suffer a −1 to all Smarts rolls, but not Smarts linked skill checks." compset="RacialProp" summary="−1 to all Smarts rolls (but not Smarts linked skill checks)." uniqueness="unique">
    <usesource source="srcRiftsTLPGAE"/>
    </thing>
  <thing id="rpSRALowLightSim" name="Low Light Vision" description="Simvan ignore Dim and Dark Illumination penalties." compset="RacialProp" summary="No penalties for Dim/Dark lighting" uniqueness="unique">
    <usesource source="srcRiftsTLPGAE"/>
    </thing>
  <thing id="rpSRAMonBeaAffSim" name="Monstrous Beast Affinity" description="When using {i}beast friend{/i}, Simvan pay half the cost for a creature’s Size (round up) and the power affects “unnatural” beasts." compset="RacialProp" summary="{i}beast friend{/i} is 1/2 cost for Size and can affect &quot;unnatural&quot; beasts/">
    <usesource source="srcRiftsTLPGAE"/>
    </thing>
  <thing id="rpSRANatPsiSim" name="Natural Psionics" description="Start with Arcane Background (Psionics), beginning with{i}beast friend/i} and two powers chosen from the standard psionic list (pg 120). If a Simvan chooses a framework providing Arcane Background (Psionics) he starts with {i}beast friend/i} and adds two powers to his starting total (e.g., a Simvan Burster starts with five powers). Simvan have a bonus of +5 ISP naturally; this bonus ISP is not part of their base ISP nor is it modified by other Edges or Abilities (e.g., a Simvan Mind Melter would begin with 35 ISP)." compset="RacialProp" summary="+5 ISP, AB: Psionics, {i}beast friend{/i}">
    <usesource source="srcRiftsTLPGAE"/>
    <bootstrap thing="edgArcPsiSWADE"></bootstrap>
    <bootstrap thing="powBeastFrSWADE">
      <autotag group="Arcane" tag="arcPsionicSWADE"/>
      </bootstrap>
    <eval phase="Setup" priority="1000">doneif (hero.tagis[User.BonusISPPowers] = 0)

#resmax[arcPsionicSWADE] += 2</eval>
    <eval phase="PreTraits" priority="6000" index="3">perform hero.childfound[arcPsionicSWADE].setfocus
doneif (state.isfocus = 0)
focus.field[trkMax].value += 5</eval>
    </thing>
  <thing id="rpSRANearHSim" name="Near-Human Physiology" description="Those unfamiliar with Simvan biology suffer a −1 penalty to Healing skill rolls and to any cybernetics checks involving installation, repair, and modification." compset="RacialProp" summary="−1 penalty to Healing skill rolls and to any cybernetics checks involving installation, repair, and modification.">
    <usesource source="srcRiftsTLPGAE"/>
    </thing>
  <thing id="rpSRARacEneSim" name="Racial Enemy" description="Psi-Stalker and Simvan tribes have been at war for generations. Meetings start argumentative and get worse. Simvan suffer −2 Persuasion with Psi-Stalkers." compset="RacialProp" summary="−2 Persuasion with Psi-Stalkers.">
    <usesource source="srcRiftsTLPGAE"/>
    <tag group="User" tag="Activation"/>
    </thing>
  <thing id="rpSRAAlienPhysTri" name="Alien Physiology" description="Trimadore are quite alien, giving those unfamiliar with their biology a −2 penalty to Healing and cybernetics checks. Juicer and Crazy augmentations don’t work for this race. Compatible medical supplies/services and cybernetics cost double the listed price." compset="RacialProp">
    <usesource source="srcRiftsTLPGAE"/>
    <eval phase="Setup" priority="500">~ assign hero the double cost tag for Cyberware
perform hero.assign[Cyberware.DoubleCost]

~ assign hero the non-standard build tag
perform hero.assign[Helper.NonStdBuildMaj]
~ assign hero the invalid augments tag
perform hero.assign[HelperRiftsAE.AugmentInvalid]</eval>
    </thing>
  <thing id="rpSRABigBoneTri" name="Big Boned" description="Trimadore have a difficult time finding armor and other gear that fits them. Purchases and repairs cost triple the listed price, and there’s no way they can manage Glitter Boy suits or other common power armor. They suffer −2 to checks made to operate equipment not refitted for their race. Starting Gear is custom-built for the character." compset="RacialProp">
    <usesource source="srcRiftsTLPGAE"/>
    </thing>
  <thing id="rpSRADBeeMajorTri" name="D-Bee (Major)" description="The initial reactions to Trimadore typically start at Unfriendly, or Hostile for human supremacists. Those with an Unfriendly or lower Reaction will refuse to sell wares or provide services such as healing, repairs, etc. Failed social checks with Hostile parties often result in violence." compset="RacialProp" summary="Initial reactions begin at Unfriendly (or Hostile for Human supremacists.">
    <usesource source="srcRiftsTLPGAE"/>
    </thing>
  <thing id="rpSRALongArmsTri" name="Long Arms" description="The Trimadore have a Reach of 1." compset="RacialProp" summary="Reach of 1.">
    <usesource source="srcRiftsTLPGAE"/>
    <eval phase="Setup" priority="2000"><![CDATA[foreach pick in hero from WeapMelee where "!thingid.wpUnarmed"

  eachpick.field[wpReach].value += 1

  nexteach


hero.childfound[wpUnarmed].field[wpReach].value += 1]]></eval>
    </thing>
  <thing id="rpSRAMechSavTri" name="Mechanical Savant" description="Trimadore are born with an innate capacity for working with and repairing machines. They begin with the Mr. Fix It Edge, ignoring any requirements." compset="RacialProp">
    <usesource source="srcRiftsTLPGAE"/>
    <tag group="Hide" tag="Special"/>
    <bootstrap thing="edgMrFixIt">
      <autotag group="thing" tag="skipprereq"/>
      </bootstrap>
    </thing>
  <thing id="rpSRATechSavTrim" name="Technically Savvy" description="Trimadore love science and technology, studying all they can about such things from birth. Trimadore start with Repair d6 and their choice of two of the following at d6: Electronics, Hacking, or Science." compset="RacialProp" summary="Repair at d6; two of Electronics, Hacking, Science at d6">
    <usesource source="srcRiftsTLPGAE"/>
    <bootstrap thing="skRepair"></bootstrap>
    <eval phase="PreTraits" priority="5000">~set resRaceSkill1 to make 2 selections
#resmax[resRaceSkill1] += 2

~Repair at D6 for free

#traitcreation[skRepair] += 1

hero.childfound[resSkill].field[resSpent].value -= 1</eval>
    <eval phase="Render" priority="10000" index="2"><![CDATA[var header as string
var chooser as string
var additem as string

additem = "Add Skill"
header = "Skill: "
header &= hero.child[resRaceSkill1].field[resSummary].text

chooser = "Skill.skKnow?"

~set up chooser expressions for Breed Advantages
perform hero.child[RacialHelper1].setfocus
focus.field[rhSkillAdd].text = additem
focus.field[rhSkillExp].text = chooser
focus.field[rhSkillHdr].text = header]]></eval>
    </thing>
  <thing id="rpSRAArmorHideDra" name="Armored Hide" description="The Hatchling has +18 MDC Armor and +6 Toughness in dragon form (plus the Size bonus)." compset="RacialProp" summary="+18 Armor (MDC), +6 Toughness">
    <fieldval field="raceValue" value="6"/>
    <fieldval field="raceUsrVal" value="18"/>
    <usesource source="srcRiftsTLPGAE"/>
    <eval phase="Setup" priority="1000"><![CDATA[~Adjust Natural Armor using raceUsrVal field
~Adjust Toughness using raceValue field
if (hero.tagis[HelperRiftsAE.DragonForm] <> 0) then
  hero.childfound[armSRANatural].field[defDefense].value += field[raceUsrVal].value
  perform #traitadjust[trTough,+,field[raceValue].value,"Armored Hide"]
  endif]]></eval>
    </thing>
  <thing id="rpSRABiteClawsDra" name="Bite/Claws" description="Dragons can naturally attack with massive teeth and talons dealing Str+3d4 Mega Damage, AP equal to Size, see {b}Natural Weapons{/b} in {i}Savage Worlds{/i}. In addition, their bite has Reach equal to the additional Wounds granted by Size (one at Size Large)." compset="RacialProp" summary="Bite &amp; Claws doing 3d4 Mega Damage. Claw AP = Size, Bite Reach = Additional wounds from  Size.">
    <usesource source="srcRiftsTLPGAE"/>
    <bootstrap thing="wpSRABite">
      <containerreq phase="Initialize" priority="900">hero#HelperRiftsAE.DragonForm</containerreq>
      <assignval field="wpDamage" value="Str+3d4"/>
      </bootstrap>
    <bootstrap thing="wpSRAClaws">
      <containerreq phase="Initialize" priority="900">hero#HelperRiftsAE.DragonForm</containerreq>
      <assignval field="wpDamage" value="Str+3d4"/>
      </bootstrap>
    <eval phase="Final">var clawAP as number
clawAP = #trait[trSize]

hero.childfound[wpSRAClaws].field[wpPiercing].value = clawAP

var sizebonus as number
Call SizeWounds
hero.childfound[wpSRABite].field[wpReach].value = sizebonus</eval>
    </thing>
  <thing id="rpSRAFearDragon" name="Fear" description="Hatchlings are terrifying creatures and cause Fear checks to all who see them." compset="RacialProp" summary="Cause Fear checks to all who see them.">
    <usesource source="srcRiftsTLPGAE"/>
    </thing>
  <thing id="rpSRAExpAwareDra" name="Expanded Awareness" description="Dragons have the {i}detect arcana{/i} power as an Innate Ability (see page 68) with the reduced Power Points cost from both the Range (Self) and Aspect limitations (see Trappings in Savage Worlds). They may also sense supernatural beings within line of sight with a Notice check." compset="RacialProp" summary="{i}detect arcana{/i} power as an Innate Ability.">
    <usesource source="srcRiftsTLPGAE"/>
    <bootstrap thing="powSRADetArcSelf">
      <autotag group="Arcane" tag="arcSRAInnateAbil"/>
      <assignval field="powTraps" value="Gestures"/>
      </bootstrap>
    <bootstrap thing="arcSRAInnateAbil"></bootstrap>
    <eval phase="Setup" priority="1000">~perform hero.delete[PowerDeny.powSRADetArcSelf]</eval>
    </thing>
  <thing id="rpSRAMetaLtdDra" name="Limited Metamorphosis" description="A dragon begins life able to adopt a single human or humanoid form for a limited amount of time each day. He can maintain this form for a number of hours equal to his Spirit die before needing at least six hours of rest. The dragon gains no special properties of what he mimics, only the size and appearance, but he retains all of his own abilities except those only usable in dragon form. Weapons and other personal effects (up to Body Armor) are assumed into the dragon’s form and reappear when returning to humanoid form. Any other objects are dropped. The process does not work in reverse; all gear carried by the dragon is dropped when transforming into humanoid form." compset="RacialProp" summary="Dragon can change into a single human/humanoid form for Spirit Die hours.">
    <fieldval field="usrChkText" value="Human Form"/>
    <usesource source="srcRiftsTLPGAE" parent="sepRiftsAE" name="Savage Rifts Core"/>
    <tag group="User" tag="Activation" name="Activation" abbrev="Activation"/>
    <tag group="HelperRiftsAE" tag="DragonForm" name="Dragon Form" abbrev="Dragon Form"/>
    <tag group="HelperRiftsAE" tag="HumanForm" name="Human Form" abbrev="Human Form"/>
    <bootstrap thing="armSRANatural">
      <containerreq phase="Initialize" priority="900">hero#HelperRiftsAE.DragonForm</containerreq>
      <autotag group="ArmorSpecial" tag="MegaDamage"/>
      <assignval field="defDefense" value="0"/>
      </bootstrap>
    <eval phase="Initialize" priority="500">~ if box is unchecked, then in dragon form, if checked, in human form
if (field[usrIsCheck].value = 0) then
  perform forward[HelperRiftsAE.DragonForm]
else
  perform forward[HelperRiftsAE.HumanForm]
endif</eval>
    </thing>
  <thing id="rpSRASizeDra" name="Size" description="In their natural forms, Dragon Hatchlings are usually 15 –20 feet from snout to tail-tip and weigh up to 7 tons. This means Hatchlings gain a fourth Wound level and, when not using their Metamorphosis abilities, +6 Toughness (in addition to the Armored Hide bonus)." compset="RacialProp" uniqueness="unique">
    <fieldval field="raceValue" value="6"/>
    <usesource source="srcRiftsTLPGAE"/>
    <eval phase="Render" priority="1000"><![CDATA[field[livename].text = field[name].text & " " & signed(field[raceValue].value)
field[shortname].text = field[livename].text]]></eval>
    <eval phase="PreTraits" priority="5000" index="2" name="Adjust Size For Race"><![CDATA[if (hero.tagis[source.ShowSize] <> 0) then
  perform #traitadjust[trSize,+,field[raceValue].value,"Dragon Size"]
else
  perform #traitadjust[trTough,+,field[raceValue].value,"Dragon Size"]
  endif]]></eval>
    </thing>
  <thing id="rpSRAFlightDra" name="Flight" description="Fly at Pace 12 and “ run” for extra movement as usual; use Athletics when maneuvering." compset="RacialProp" summary="May Fly at Pace 12" uniqueness="unique">
    <usesource source="srcRiftsTLPGAE"/>
    <eval phase="Traits" priority="5000">hero.child[trPace].field[trtSpecial].value = 12</eval>
    </thing>
  <thing id="rpSRAInfravisDra" name="Infravision" description="Dragons halve penalties for bad lighting when attacking targets that radiate warmth." compset="RacialProp" summary="1/2 lighting penalties vs targets radiating warmth." uniqueness="unique">
    <usesource source="srcRiftsTLPGAE"/>
    </thing>
  <thing id="rpSRAInhMagDra" name="Inherently Magical" description="Hatchlings know no magic spells, but they have a natural pool of 10 PPE and can use Techno-Wizard gear. A dragon who takes an Arcane Background adds this starting PPE to any gained from the Edge. Additionally, whenever he takes the Power Points Edge, he gains +10 PPE instead of +5." compset="RacialProp">
    <usesource source="srcRiftsTLPGAE"/>
    <eval phase="PreTraits" priority="5000">~ if no Arcane.Magic on hero, then add PPE to Innate
if (hero.tagis[Arcane.Magic] = 0) then
  perform hero.childfound[arcSRAInnateAbil].setfocus
  perform focus.assign[Helper.UsesPPE]
else
  perform hero.childfound[arcMagicSWADE].setfocus
  endif

doneif (state.isfocus = 0)

focus.field[trkMax].value += 10</eval>
    </thing>
  <thing id="rpSRAPsiMinDra" name="Minor Psionic" description="Dragons begin with Arcane Background (Psionics), 10 ISP, Psionics d4, speak language and t wo other powers chosen from the standard psionic power list on page 120." compset="RacialProp" summary="10 ISP, Psionics d4, Speak language and two other powers">
    <usesource source="srcRiftsTLPGAE"/>
    <bootstrap thing="edgArcPsiSWADE"></bootstrap>
    <bootstrap thing="powSpeakSWADE">
      <autotag group="Arcane" tag="arcPsionicSWADE"/>
      </bootstrap>
    <eval phase="Initialize" priority="2000"><![CDATA[~ Call setup procedure for power list

~ clearPwr - 0 = don't clear, 1 = clear all Powers

var clearPwrs as number

clearPwrs = 1

call GiveStdPsionicList]]></eval>
    </thing>
  <thing id="rpSRANighImmDra" name="Nigh-Immortality" description="Dragons are supernatural creatures thought to be as old as the cosmos itself. Hatchlings mature around 600 years of age and, unless killed, can live up to 12,000 years. They are sustained by passively and effortlessly absorbing ambient magic energy, eating and drinking simply for pleasure. Dragons are immune to the Hazards of cold, heat, hunger, thirst, disease, poison, drowning, electricity, and radiation; see Hazards in Savage Worlds." compset="RacialProp" summary="Immune to the Hazards of cold, heat, hunger, thirst, disease, poison, drowning, electricity, and radiation. 12,000 yr lifespan.">
    <usesource source="srcRiftsTLPGAE"/>
    </thing>
  <thing id="rpSRASlowRegenDra" name="Slow Regeneration" description="Dragons make a natural healing roll once per day. Permanent injuries may be recovered once all Wounds are regenerated; treat each injury as an additional Wound for purposes of recovery." compset="RacialProp">
    <usesource source="srcRiftsTLPGAE"/>
    </thing>
  <thing id="rpSRATailLashDra" name="Tail Lash" description="A Hatchling in dragon form can attack all opponents adjacent to its rear or side as an action. The Hatchling uses its Athletics skill and ignores Scale penalties for this particular attack, which may be Evaded. Those who fail suffer Str+d8 Mega Damage." compset="RacialProp" summary="Sweep all ppponents to rear or side using Athletics skill (ignoring Scale). May be Evaded.">
    <usesource source="srcRiftsTLPGAE"/>
    </thing>
  <thing id="racSRAHuman" name="Human" description="Humans are the standard by which all others are measured. Not because they are the oldest, wisest, or strongest of the races, but because they are the most widespread and varied.{br}{br}Humans come in all varieties, from savage warriors of distant jungles to wise priests and holy crusaders.{br}{br}Humans live a little over half a century in low-tech worlds, and nearly twice that in the modern age. Their skin color ranges from dark brown to the color of snow, and their eyes are blue, green, brown, or some mix of the three." compset="Race" replaces="racHuman" uniqueness="unique">
    <usesource source="srcRiftsTLPGAE"/>
    <bootstrap thing="rpSRABonusEdge"></bootstrap>
    <containerreq phase="Initialize" priority="2100">!hero#Hero.Creature</containerreq>
    </thing>
  <thing id="fcSRACyberDra" name="Cybernetics" description="Dragons cannot take cybernetics at any point. Their bodies reject such foreign implants." compset="FraComp">
    <usesource source="srcRiftsTLPGAE"/>
    <eval phase="Setup" priority="500">~ assign hero the invalid augments tag
perform hero.assign[HelperRiftsAE.AugmentInvalid]
perform hero.assign[HelperRiftsAE.CyberNotValid]</eval>
    </thing>
  <thing id="fcSRAEnemiesDra" name="Enemies" description="Dragons are reviled by the Coalition, and hunted with extreme prejudice by CS forces when possible. In addition, many arcane factions seek to control dragons or use their body for the creation of powerful items." compset="FraComp">
    <usesource source="srcRiftsTLPGAE"/>
    </thing>
  <thing id="fcSRAFormLimDra" name="Form Limits" description="Dragons are reviled by the Coalition, and hunted with extreme prejudice by CS forces when possible. In addition, many arcane factions seek to control dragons or use their body for the creation of powerful items." compset="FraComp">
    <fieldval field="frCostValue" value="1"/>
    <usesource source="srcRiftsTLPGAE"/>
    </thing>
  <thing id="fcSRAHatchedDra" name="Hatched" description="Dragons are born not made. They do not choose a race." compset="FraComp">
    <usesource source="srcRiftsTLPGAE"/>
    </thing>
  <thing id="fcSRAOutsidMajDra" name="Outsider (Major)" description="Dragon Hatchlings understand so little of the world, they inevitably cause socially awkward situations wherever they go, even in humanoid form. All Dragon Hatchlings have the Outsider (Major) Hindrance." compset="FraComp" summary="All Dragon Hatchlings have the Outsider (Major) Hindrance.">
    <usesource source="srcRiftsTLPGAE"/>
    <tag group="Hide" tag="Special"/>
    <bootstrap thing="hinOutside"></bootstrap>
    </thing>
  <thing id="fcSRATerritDra" name="Territorial" description="Dragons tend to dislike their own, at least initially. Dragons are hierarchical and generally treat newcomers as invading their territory unless great care is taken in interactions." compset="FraComp" summary="Dragons are hierarchical and generally treat newcomers as invading their territory">
    <usesource source="srcRiftsTLPGAE"/>
    </thing>
  <thing id="fcSRAUntestDra" name="Untested" description="Being so young and without any life experiences, Dragon Hatchlings have not had time to develop moral codes, physical limitations, or foibles. When creating a Dragon Hatchling character, the player selects no Hindrances to start (and, thus, doesn’t get the extra points to spend on character creation). Instead, he selects one Hindrance at the end of each session until he has four points worth of Hindrances. Only when all Hindrances are selected may the player spend the points on his character, working out what is appropriate with the Game Master. This represents imprinting and a “growth spurt” for the Dragon Hatchling." compset="FraComp" summary="Start with no Hindrances. May add one per session but no points gained until 4 points worth selected.">
    <usesource source="srcRiftsTLPGAE"/>
    </thing>
  <thing id="fcSRAVeryYngDra" name="Very Young" description="Dragon Hatchlings start out very young. They begin play speaking and reading only one language, Dragonese/Elven, and start with only 3 points to adjust their attributes and only 10 skill points. Unlike the Young Hindrance (which they cannot take), this complication does not grant extra Bennies each session." compset="FraComp" summary="Start with no Hindrances. May add one per session but no points gained until 4 points worth selected.">
    <usesource source="srcRiftsTLPGAE"/>
    <eval phase="Final" priority="5100" index="2">#resleft[resLang] = 0
#resmax[resLang]  = 1</eval>
    <eval phase="PreTraits" priority="5000">#resmax[resAttrib] -= 2
#resmax[resSkill] -= 5</eval>
    </thing>
  <thing id="rpSRABadRepMinVer" name="Bad Reputation (Minor)" description="The “Serpent People” of the Southwest are infamous for taking pleasure in abusing, tricking, and feasting on humanoids; initial Reactions are a level worse than usual." compset="RacialProp">
    <usesource source="srcRiftsTLFMAE"/>
    </thing>
  <thing id="rpSRABizPhysVer" name="Bizarre Physiology" description="Vernulian physiology is quite bizarre, those unfamiliar are −4 penalty to Healing checks. Compatible medical supplies/services and cybernetics cost double the listed price." compset="RacialProp">
    <usesource source="srcRiftsTLFMAE"/>
    <eval phase="Setup" priority="500">~ assign hero the double cost tag for Cyberware
perform hero.assign[Cyberware.QuadCost]

~ assign hero the non-standard build tag
perform hero.assign[Helper.NonStdBuildMaj]</eval>
    </thing>
  <thing id="rpSRADBeeMajVer" name="D-Bee (Major)" description="The initial Reactions to Vernulians typically start at Hostile, and human supremacists are violent unless a social skill check is successful." compset="RacialProp" summary="Initial reactions begin at Hostile (or immediate violence for Human supremacists.">
    <usesource source="srcRiftsTLFMAE"/>
    </thing>
  <thing id="rpSRAFourArmVer" name="Four Arms" description="Vernulians ignore 2 points of Multi-Action penalties each turn." compset="RacialProp" uniqueness="unique">
    <usesource source="srcRiftsTLFMAE"/>
    </thing>
  <thing id="rpSRAWeirdWrldVer" name="&quot;It&apos;s A Weird World" description="Vernulians are still struggling to adjust to the odd realities of Rifts Earth, where they are often attacked as monsters. Each Vernulian reacts to this in their own way, and starts with the Cautious, Mean, Pacifist (Minor), or Suspicious (Minor) Hindrance for no added benefit." compset="RacialProp">
    <usesource source="srcRiftsTLFMAE"/>
