    </thing>
  <thing id="racSRAIntelConstr" name="Intelligent Construct" compset="Race" uniqueness="unique">
    <usesource source="srcRiftsEmpHumAE"/>
    <bootstrap thing="rpSRAConstruct"></bootstrap>
    <bootstrap thing="rpSRAInorganic"></bootstrap>
    <bootstrap thing="rpSRACoreDirecMaj"></bootstrap>
    <bootstrap thing="rpSRAOutsiderMaj"></bootstrap>
    <bootstrap thing="rpSRAWantedMajIC"></bootstrap>
    <bootstrap thing="rpSRAMalfunctions"></bootstrap>
    <bootstrap thing="resICPoints"></bootstrap>
    <bootstrap thing="resICMindSel"></bootstrap>
    <bootstrap thing="resICTechSel"></bootstrap>
    <bootstrap thing="resICFrameSel"></bootstrap>
    <bootstrap thing="resICLocoSel"></bootstrap>
    <bootstrap thing="resICSenseSel"></bootstrap>
    <bootstrap thing="resICAppearSel"></bootstrap>
    <bootstrap thing="resICVocalSel"></bootstrap>
    <bootstrap thing="valICHardSkill"></bootstrap>
    <eval phase="Setup" priority="1000">~begin with no Hero Journey selections
#resmax[resHeroJrn] -= 2

~begin with 24 points for construct abilities
#resmax[resICPoints] += 24

~selections
#resmax[resICMindSel] += 1
#resmax[resICTechSel] += 1
#resmax[resICFrameSel] += 1
#resmax[resICLocoSel] += 1
#resmax[resICSenseSel] += 1
#resmax[resICAppearSel] += 1
#resmax[resICVocalSel] += 1

~ Education, Psionics and Training are the only valid HJ tables
~perform hero.delete[HJInvalidCatSRA.Edu]
~perform hero.delete[HJInvalidCatSRA.Psi]
~perform hero.delete[HJInvalidCatSRA.Train]</eval>
    </thing>
