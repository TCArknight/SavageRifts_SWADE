  <thing id="hjSRAUndSocial" name="Socializer" description="She’s traveled far and wide, and she always seems to know someone, somewhere, she can call on for information or aid. Your hero has the I Know a Guy Edge and gains +2 on all Networking rolls to contact and gain assistance." compset="HerosJourney" summary="Gain I Know A Guy edge and a +2 on all Networking rolls to contact and gain assistance.">
    <fieldval field="hjRangeMin" value="5"/>
    <fieldval field="hjRangeMax" value="7"/>
    <usesource source="srcRiftsTLPGAE"/>
    <tag group="HJCategorySRA" tag="Under"/>
    <bootstrap thing="edgSRAIKnowAGuy"></bootstrap>
    </thing>
  <thing id="hjSRAUndDirtyDirt" name="Dirty Fighting (Dirty Fighter)" description="Your character has a knack for taking full advantage of his opponents in all manner of desperate circumstances. He has the Dirty Fighter Edge, or Tricky Fighter if he already has Dirty Fighter." compset="HerosJourney" summary="Begin with Dirty Fighter edge.">
    <fieldval field="hjRangeMax" value="8"/>
    <fieldval field="hjRangeMin" value="8"/>
    <usesource source="srcRiftsTLPGAE"/>
    <tag group="HJCategorySRA" tag="Under"/>
    <tag group="Hide" tag="Special"/>
    <bootstrap thing="edgSRADirtyFightr"></bootstrap>
    <pickreq thing="edgSRADirtyFightr" ispreclude="yes"/>
    </thing>
  <thing id="hjSRAUndDirtyTri" name="Dirty Fighting (Tricky Fighter)" description="Your character has a knack for taking full advantage of his opponents in all manner of desperate circumstances. He has the Dirty Fighter Edge, or Tricky Fighter if he already has Dirty Fighter." compset="HerosJourney" summary="Begin with Tricky Fighter edge.">
    <fieldval field="hjRangeMax" value="8"/>
    <fieldval field="hjRangeMin" value="8"/>
    <usesource source="srcRiftsTLPGAE"/>
    <tag group="HJCategorySRA" tag="Under"/>
    <tag group="Hide" tag="Special"/>
    <bootstrap thing="edgSRATrickyFight"></bootstrap>
    <pickreq thing="edgSRATrickyFight" ispreclude="yes"/>
    </thing>
  <thing id="hjSRAUndStreet" name="Streetwise" description="Though few and far between, the cites of Rifts Earth are the hubs of what remains of civilization, vital as sources of information. Your character understands the ebb and flow of the streets and alleys, gaining the Streetwise Edge. She is also good at creating fakes of necessary papers, badges, and the like; she’s got experience and training in forgery (treat as Common Knowledge), with a +2 to related checks." compset="HerosJourney" summary="Begin with the Streetwise edge. Also +2 on any Forgery (treat as Common Knowledge) rolls." uniqueness="useronce">
    <fieldval field="hjRangeMax" value="10"/>
    <fieldval field="hjRangeMin" value="9"/>
    <usesource source="srcRiftsTLPGAE"/>
    <tag group="HJCategorySRA" tag="Under"/>
    <bootstrap thing="edgStreetwise">
      <autotag group="thing" tag="skipprereq"/>
      </bootstrap>
    </thing>
  <thing id="hjSRAUndThief" name="Thief" description="Your character is inherently gifted at larceny, possessing the Thief Edge without concern for requirements. He also gains +1 Skill Point to spend on Athletics, Stealth, or Thievery as he chooses." compset="HerosJourney" summary="Gain the Thief edge and 1 skill point to spend on Athletics, Stealth, or Thievery">
    <fieldval field="hjRangeMin" value="11"/>
    <fieldval field="hjRangeMax" value="13"/>
    <usesource source="srcRiftsTLPGAE"/>
    <tag group="HJCategorySRA" tag="Under"/>
    <eval phase="PreTraits" priority="5000">~+1 Skill Points
#resmax[resSkill] += 1</eval>
    <evalrule phase="Validate" priority="5000" message="Spend 1 Skill Point on Athletics, Stealth or Thievery." summary="Spend 1 Skill Point on Athletics, Stealth or Thievery."><![CDATA[var totalcount as number
      totalcount = 0

      foreach pick in hero where "thingid.skAthletic|thingid.skStealth|thingid.skThievery"
        if (eachpick.field[trtFinal].value >= 2) then
          totalcount += (eachpick.field[trtUser].value - 1)
          endif
        nexteach
		
      validif (totalcount >= 1)]]></evalrule>
    </thing>
  <thing id="hjSRAUndInfiltrat" name="Infiltrator" description="Your hero is an infiltration specialist, skilled at blending into a culture or group for undercover work. She gains a +1 to any Persuasion, Research, or Stealth rolls related to being undercover, and a +2 to any Common Knowledge rolls for disguising herself and playing the role." compset="HerosJourney" summary="Gains a +1 to any Persuasion, Research, or Stealth rolls related to being undercover, and a +2 to any Common Knowledge rolls for disguising herself and playing the role.">
    <usesource source="srcRiftsTLPGAE"/>
    <tag group="HJCategorySRA" tag="Under"/>
    </thing>
  <thing id="hjSRAUndScrounger" name="Scrounger" description="With a nose for opportunities, horse-trading, and five-fingered discounts, your hero is the go-to person for getting what’s needed in a crunch. He has the Scrounger Edge." compset="HerosJourney" summary="Gain the Scrounger Edge.">
    <fieldval field="hjRangeMin" value="15"/>
    <fieldval field="hjRangeMax" value="16"/>
    <usesource source="srcRiftsTLPGAE"/>
    <tag group="HJCategorySRA" tag="Under"/>
    <tag group="Hide" tag="Special"/>
    <bootstrap thing="edgSRAScrounger">
      <autotag group="thing" tag="skipprereq"/>
      </bootstrap>
    </thing>
  <thing id="hjSRAUndJackTrade" name="Jack of All Trades" description="Spies and provocateurs are regularly called upon to do just about anything to accomplish a mission; your hero’s made it a point to learn a little something about everything. She has the Jack-of-All-Trades Edge." compset="HerosJourney" summary="Gain the Jack-of-All-Trades Edge." uniqueness="useronce">
    <fieldval field="hjRangeMax" value="17"/>
    <fieldval field="hjRangeMin" value="17"/>
    <usesource source="srcRiftsTLPGAE"/>
    <tag group="Hide" tag="Special"/>
    <tag group="HJCategorySRA" tag="Edu"/>
    <bootstrap thing="edgJackAllSWADE">
      <autotag group="thing" tag="skipprereq"/>
      </bootstrap>
    <eval phase="PreTraits" priority="5000">#traitcreation[skRepair] += 1</eval>
    </thing>
  <thing id="faSRABornSaddle" name="Born In The Saddle" description="Cowboys are expert riders with a special connection to their mounts. They ignore two points of penalties to any Riding roll and ignore the Multi- Action penalty for making a Riding check in the same round as other actions. Cowboys may spend Bennies to Soak damage for their mount using Riding instead of Vigor. Each success and raise negates a Wound. They may mount or dismount as a free action. When applying Beast Master to their chosen mount, increase the default to Size 3 (instead of Size 0)." compset="FraAbility" summary="Ignore 2 pts of Penalties to Riding, and ignore MAP for Riding with other actions. Also, may spend Bennies to Soak damage for mount.">
    <usesource source="srcRiftsEmpHumAE"/>
    </thing>
  <thing id="faSRASpiHomeCook" name="Spicy Home Cooking" description="Given time and common “household” ingredients, the EOD Specialist may roll Repair to manufacture improvised grenades, mines, and other explosive devices listed in Savage Worlds. Alternatively, the EOD Specialist can upgrade the damage of explosive devices with a Science check—a success adds an extra die of the explosive’s damage die type once detonated, and a raise allows a free reroll of the damage once detonated." compset="FraAbility" summary="Use Repair skill to manufacture improvised explosive devices. Alternately, can use Science to upgrade damage of explosive devices by 1d (2d) with a raise.">
    <usesource source="srcRiftsEmpHumAE"/>
    </thing>
  <thing id="faSRAQuickDead" name="Quick And The Dead" description="In the first round of combat, immediately after Action Cards are dealt for initiative, a Gunslinger may spend a Benny to draw on an opponent. The Gunslinger and his opponent make an opposed Agility roll. If the Gunslinger succeeds, he immediately attacks the target and has The Drop. If he fails, the Benny is wasted with no effect. The Gunslinger still acts on his Action Card as normal." compset="FraAbility" summary="In the first round of combat, immediately after Action Cards are dealt for initiative, may spend a Benny to draw on an opponent. Still acts on his Action Card as normal.">
    <usesource source="srcRiftsEmpHumAE"/>
    </thing>
  <thing id="wpSRAMagBladeSpi" name="Magical Blades and Spikes" description="TW Blades and Spikes included in Bionic Arm" compset="MeleeSRA">
    <fieldval field="wpDamage" value="Str+2d4"/>
    <fieldval field="wpPiercing" value="8"/>
    <usesource source="srcRiftsTLPGAE"/>
    <tag group="Hide" tag="Equipment"/>
    <tag group="Weapon" tag="IsClaws" name="Weapon is considered Claws"/>
    <tag group="WeaponType" tag="TWWeapon" name="Techno-Wizardry Weapon"/>
    <tag group="Equipment" tag="Natural"/>
    <tag group="Helper" tag="TWWeapon"/>
    </thing>
  <thing id="faSRABionicAugMM" name="Bionically Augmented" description="Momano gain +3 Strength, +1 Agility and +1 Vigor die types—no Trait maximums for these Attributes; bonuses come from the following TW cybernetics: Bionic Strength Augmentation (3), Cyber-Wired Reflexes (1), and Synthetic Organ Replacement (1)." compset="FraAbility">
    <usesource source="srcRiftsEmpHumAE"/>
    <bootstrap thing="cybSRABioStrAug">
      <autotag group="HelperRiftsAE" tag="TWCyberware"/>
      <assignval field="stackQty" value="3"/>
      </bootstrap>
    <bootstrap thing="cybSRACybWiredRef">
      <autotag group="HelperRiftsAE" tag="TWCyberware"/>
      </bootstrap>
    <bootstrap thing="cybSRASynthOrganR">
      <autotag group="HelperRiftsAE" tag="TWCyberware"/>
      </bootstrap>
    <bootstrap thing="wpSRAMagBladeSpi"></bootstrap>
    <bootstrap thing="wpSRAHydStakeDri"></bootstrap>
    <bootstrap thing="wpSRAEnergyProj"></bootstrap>
    </thing>
  <thing id="faSRACybTechnoWar" name="Cybernetic Techno-Warriors" description="Momano begin with the following additional cybernetic systems: Armor Plating (1 level), Core Electronics Package, Expanded Detection and Security Array, Nano-Repair System, Optics Package, Targeting Eye." compset="FraAbility">
    <usesource source="srcRiftsEmpHumAE"/>
