  <thing id="modSRASpeedVeh" name="Speed" compset="Modificatn" summary="Increases the vehicle’s Top Speed by 10%.">
    <fieldval field="grModsUsed" value="1"/>
    <fieldval field="grCost" value="1000"/>
    <usesource source="srcRiftsTLPGAE"/>
    <tag group="User" tag="SizeCost"/>
    <tag group="ModType" tag="Vehicle"/>
    <eval phase="PreTraits" priority="5500"><![CDATA[var accel as number
      var speed as number

      accel = container.parent.field[vhAccel].text
      if (field[stackQty].value > 1) then 
        accel += 5 * field[stackQty].value
      else      
        accel += 5
        endif
      container.parent.field[vhAccel].text = accel

      speed = container.parent.field[vhTopSpeed].text
      if (field[stackQty].value > 1) then 
        speed += 10 * field[stackQty].value
      else      
        speed += 10
        endif
      container.parent.field[vhTopSpeed].text = speed]]></eval>
    <evalrule phase="Validate" priority="9000" message="Speed and Speed Reduction cannot both be taken." summary="Speed and Speed Reduction cannot both be taken.">validif (container.parent.tagis[Modificatn.modVSpeedR] = 0)</evalrule>
    <prereq message="Cannot be taken with Speed Reduction">
      <validate>validif (parent.tagis[Modificatn.modVSpeedR] = 0)</validate>
      </prereq>
    </thing>
