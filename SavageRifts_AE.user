    <usesource source="srcRiftsTLPGAE"/>
    <tag group="ModType" tag="Vehicle"/>
    <tag group="User" tag="SizeCost"/>
    <eval phase="Effects" priority="5000"><![CDATA[var armor as number
      var tough as number

      armor = container.parent.field[vhArmor].text
      tough = container.parent.field[vhTough].text
      if (field[stackQty].value > 1) then 
          armor += 2 * field[stackQty].value
          tough += 2 * field[stackQty].value
        else      
          armor += 2
          tough += 2
          endif
      container.parent.field[vhArmor].text = armor
      container.parent.field[vhTough].text = tough
      if (container.parent.field[vhArmor].text > 3) then 
        perform container.parent.assign[Vehicle.MDCArmor]
        endif]]></eval>
    </thing>
