    <eval phase="Setup" priority="500">~ assign hero the double cost tag for Cyberware
perform hero.assign[Cyberware.QuadCost]

~ assign hero the non-standard build tag
perform hero.assign[Helper.NonStdBuildMaj]</eval>
    </thing>
  <thing id="rpSRADBeeMajVer" name="D-Bee (Major)" description="The initial Reactions to Vernulians typically start at Hostile, and human supremacists are violent unless a social skill check is successful." compset="ExtRacialProp" summary="Initial reactions begin at Hostile (or immediate violence for Human supremacists.">
    <usesource source="srcRiftsTLFMAE"/>
