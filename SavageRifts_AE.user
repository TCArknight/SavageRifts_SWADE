    <usesource source="srcRiftsArcMysAE"/>
    </thing>
  <thing id="rpSRAMindMattPsX" name="Mind Over Matter" description="Psi-X Aliens begin with the Arcane Background (Psionics) and Power Points Edges, 15 ISP, a Psionics skill of d6, and start with three powers: {i}mind link, telekinesis,{/i} and one other power from the Mind Melter’s list of psionics. They naturally telekinetically hover and move a few feet above the ground at their base Pace— at no cost as a free action— and must concentrate to walk on their own two legs. If a Psi-X Alien chooses an Iconic Framework providing Arcane Background (Psionics), he follows all the normal rules for the Framework except three of his starting powers are those listed above (even if not normally allowed by that Framework) and he gains two additional powers to his starting total (e.g., a Psi-X Burster starts with five powers, etc.)." compset="ExtRacialProp" summary="Begin with 15 ISP, Psionics d6, AB (Psionics) and {i}mind link, telekinesis,{/i} and one other power from the Mind Melter’s list of psionics.">
    <usesource source="srcRiftsEmpHumAE"/>
    <bootstrap thing="skPsionics"></bootstrap>
    <bootstrap thing="edgSRAArcPsionics"></bootstrap>
    <bootstrap thing="powMindLinkSWADE">
      <autotag group="Arcane" tag="arcPsionicSWADE"/>
      </bootstrap>
    <bootstrap thing="powTelekinSWADE">
      <autotag group="Arcane" tag="arcPsionicSWADE"/>
      </bootstrap>
    <eval phase="PreTraits" priority="5000">#traitcreation[skPsionics] += 1
hero.childfound[resSkill].field[resSpent].value -= 1

perform hero.childfound[arcPsionicSWADE].setfocus
doneif (state.isfocus = 0)
focus.field[trkMax].value += 5</eval>
    </thing>
  <thing id="rpSRAObsIntPsX" name="Obsessive Interest" description="Psi-X Aliens begin play with one Smarts based skill of their choice at d4 and gain +2 when using that skill." compset="ExtRacialProp" summary="Begin play with one Smarts based skill of their choice at d4. +2 to selected skill in addition.">
    <fieldval field="usrCandid1" value="component.Skill &amp; Helper.SmartsSkill"/>
    <fieldval field="usrSource1" value="2"/>
    <usesource source="srcRiftsEmpHumAE"/>
    <eval phase="PreTraits" priority="5000"><![CDATA[~apply the +2 modifier to selected skill
if (field[usrChosen1].ischosen <> 0) then
   perform field[usrChosen1].chosen.field[trtRoll].modify[+,2,"Obsessive Interest"]
endif]]>
      <before name="Calc trtFinal"/>
      </eval>
    </thing>
  <thing id="rpSRAOutOfItPsX" name="Out of It" description="The Psi-X Aliens suffer from a developmental disorder leaving them struggling to understand societal interactions and norms. They suffer a −2 to all Common Knowledge rolls." compset="ExtRacialProp" summary="−2 to all Common Knowledge rolls">
    <usesource source="srcRiftsEmpHumAE"/>
