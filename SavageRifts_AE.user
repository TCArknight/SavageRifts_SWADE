    </thing>
  <thing id="modSRAFearFDe" name="Fear" description="Fire Demon Automatons cause Fear checks when encountered." compset="Modificatn">
    <usesource source="srcRiftsArcMysAE"/>
    <tag group="ModType" tag="Walker"/>
    </thing>
  <thing id="modSRAFDeSpellcst" name="Spellcasting" description="The operator can command the Colossus to cast the following spells, using the lower of his Piloting or Spellcasting Skill. These spells draw on the Automaton’s near-unlimited PPE store, and all can be cast using Power Modifiers: {i}blast, bolt, light/darkness, smite{/i}." compset="Modificatn" summary="Cast using lower of Piloting or Spellcasting (near unlimited PPE): {i}blast, bolt, light/darkness, smite{/i}.">
    <usesource source="srcRiftsArcMysAE"/>
    <tag group="ModType" tag="Walker"/>
    </thing>
  <thing id="modSRAFDeSpecQual" name="Special Qualities" description="The Fire Demon emanates a constant aura of heat and fire damage, much like the damage field power. It does 2d8 Mega Damage to anyone who physically contacts it or hits it in melee (and this damage is added to its own melee attacks). This aura does not affect the Controller in any way. The Fire Demon also has Fast Regeneration (at Vigor d12), but the operator must spend 10 PPE of his own to activate it, and it only lasts five rounds per activation. Fire Demon Automatons (and their Controllers) have Immunity to all flame and heat attacks." compset="Modificatn" summary="Heat/Fire Aura (Dmg Field (2d8MD)); Fast Regen (5 rnds, VIG d12, 10PPE from Controller to activate), Immune to Flame/Heat attacks.">
    <usesource source="srcRiftsArcMysAE"/>
    <tag group="ModType" tag="Walker"/>
    </thing>
  <thing id="wpSRAFireBreath" name="Fire Breath" compset="SpecWeapSRA">
    <fieldval field="wpDamage" value="3d12"/>
    <fieldval field="wpPiercing" value="10"/>
    <usesource source="srcRiftsArcMysAE"/>
    <tag group="Weapon" tag="MegaDamage"/>
    <tag group="WeaponType" tag="SFVehAntiP"/>
    <tag group="Hide" tag="Equipment"/>
    <tag group="Equipment" tag="ModSlotUse"/>
    <tag group="Weapon" tag="ConeTempl"/>
    <tag group="Weapon" tag="ItBurns"/>
    <eval phase="Initialize" priority="2000">doneif (ispick = 0)
doneif (isuser = 0)

perform assign[Weapon.SnapFire]
perform assign[Equipment.TwoHand]</eval>
    </thing>
  <thing id="wpSRAFlamingHands" name="Flaming Hands" compset="SpecWeapSRA">
    <fieldval field="wpDamage" value="Str+3d4"/>
    <fieldval field="wpPiercing" value="12"/>
    <usesource source="srcRiftsArcMysAE"/>
    <tag group="Weapon" tag="MegaDamage"/>
    <tag group="WeaponType" tag="SFVehMelee"/>
    <tag group="Hide" tag="Equipment"/>
    <tag group="Equipment" tag="ModSlotUse"/>
    </thing>
  <thing id="faSRAEnerDiminish" name="Energetic Diminishment" description="While protection is activated, the Psi-Slinger takes −4 damage from energy attacks (electricity, fire, ion, lasers, particle beams, plasma, etc.)." compset="FraAbility">
    <usesource source="srcRiftsArcMysAE"/>
    </thing>
  <thing id="faSRATelekinEnh" name="Telekinetic Enhancement" description="When imbuing a Modern Firearm (see {i}Savage Worlds{/i}) or TW equivalent with {i}smite{/i}, the weapon does one additional die of damage. So, a Colt Peacemaker empowered with base smite by a Psi-Slinger does 3d6+3 damage." compset="FraAbility">
    <usesource source="srcRiftsArcMysAE"/>
    </thing>
  <thing id="powSRATraitSelf" name="Boost/Lower Trait (Self)" description="This power allows a character to increase or decrease a target’s Trait (attribute or skill).\n\n{i}Boosting{/i} an ally’s Trait increases the selected Trait one die type, or two with a raise, for five rounds.\n\n{i}Lowering{/i} an enemy’s Trait has a Duration of Instant and lowers the selected attribute or skill a die type with success, or two with a raise (to a minimum of d4). A victim automatically tries to shake off the effect with a Spirit roll as a free action at the end of his following turns. Success improves the effect one die type, and a raise removes the effect entirely.\n\nAdditional castings don’t stack on a single Trait (take the highest), but may affect {i}different{/i} Traits.\n\n{b}MODIFIERS{/b}\n{indent -10}   • ADDITIONAL RECIPIENTS (+1): The power may affect more than one target for 1 additional Power Point each.\n   • STRONG (+1): Lower Trait only. The Spirit roll to shake off the effect is made at −2." compset="Power" summary="Raise/lower a trait by one die-type (or two with raise)">
    <fieldval field="powPoints" value="1"/>
    <fieldval field="powRange" value="Self"/>
    <fieldval field="powLength" value="5/Instant"/>
    <fieldval field="powTraps" value="Physical change, glowing aura, potions"/>
    <usesource source="srcRiftsTLPGAE"/>
    <tag group="MinRank" tag="0"/>
    </thing>
  <thing id="faSRAVigorPsiWar" name="Vigor" description="Psi-Warriors gain +2 on all Vigor checks to resist Cold, Heat, Hunger, Sleep, or Thirst." compset="FraAbility">
    <usesource source="srcRiftsArcMysAE"/>
    </thing>
  <thing id="faSRAArtsOffense" name="The Arts of Offense" description="The Psi-Warrior&apos;s Psi-Blade damage dice count as Claws when applying bonuses from Edges." compset="FraAbility">
    <usesource source="srcRiftsArcMysAE"/>
    </thing>
  <thing id="faSRANatHealing" name="Natural Healing" description="Spirit Warriors may make a natural healing roll once per day (instead of every five days). Permanent injuries may be recovered once all other Wounds are regenerated—treat each injury as an additional Wound (may try once per week)." compset="FraAbility" summary="May make a natural healing roll once per day (instead of every five days).">
    <usesource source="srcRiftsArcMysAE"/>
    </thing>
