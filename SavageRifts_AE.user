    <tag group="Vehicle" tag="LaserResist" name="Laser Resistant (1/2 dmg/AP from lasers)"/>
    <tag group="VehType" tag="PowerAr"/>
    <tag group="Vehicle" tag="LanguageTran" name="Language Translator"/>
    <tag group="Vehicle" tag="DepthGauge" name="Depth Gauge"/>
    <tag group="Vehicle" tag="AdvComms20" name="Advanced Comms (20 (500) mile range)"/>
    <tag group="Vehicle" tag="SensorSuit"/>
    <tag group="Vehicle" tag="Targeting"/>
