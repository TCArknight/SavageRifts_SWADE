    <eval phase="PreTraits" priority="5000">hero.child[attrStr].field[trtMaximum].value += 99
hero.child[attrAgi].field[trtMaximum].value += 99
hero.child[attrVig].field[trtMaximum].value += 99</eval>
    </thing>
  <thing id="faSRAHarderSteel" name="Harder than Steel" description="The full conversion ’Borg&apos;s construction provides +6 MDC natural Armor and +3 Toughness, which is the maximum level possible for both cybernetic Armor Plating and Reinforced Frame (see Cybernetic Enhancements, above). These Armor and Toughness bonuses do stack at their full value with any Worn Armor providing MDC Armor, but do not stack with non-MDC Armor." compset="FraAbility">
    <usesource source="srcRiftsTLPGAE"/>
    </thing>
  <thing id="faSRAHighperfLegs" name="High-performance Legs" description="Full-conversion ’borgs begin with the Fleet-Footed Edge." compset="FraAbility">
    <usesource source="srcRiftsTLPGAE"/>
    <bootstrap thing="edgFleetFooted"></bootstrap>
    </thing>
  <thing id="faSRAMoreMachine" name="More Machine" description="&apos;Borgs add +2 to recover from Shaken and Stunned, ignore one level of Wound modifiers, and possess the Internal Life Support cybernetic enhancement: they have an internal air supply and other systems that make him immune to disease, drowning, as well as poison and grants +4 to resist cold, heat, and radiation. \n\nCombat Cyborgs only require a fifth of the nutrition of a fully organic person." compset="FraAbility">
    <usesource source="srcRiftsTLPGAE"/>
    </thing>
  <thing id="faSRASizeCbtCyb" name="Size 1" description="Combat Cyborgs are big and heavy, gaining +1 Toughness from their Size." compset="FraAbility">
    <usesource source="srcRiftsTLPGAE"/>
    <eval phase="PreTraits" priority="5000"><![CDATA[if (hero.tagis[source.ShowSize] <> 0) then
  perform #traitadjust[trSize,+,1,"Size"]
else
  perform #traitadjust[trTough,+,1,"Size"]
  endif]]></eval>
    </thing>
  <thing id="faSRAUpgradeable" name="Upgradeable" description="’Borgs start with the Upgradeable Edge." compset="FraAbility">
    <usesource source="srcRiftsTLPGAE"/>
    <bootstrap thing="edgSRAUpgradeable"></bootstrap>
    </thing>
  <thing id="faSRAUnarmedCbtCy" name="Unarmed Combat" description="Combat Cyborgs deal Str+d4 Mega Damage in unarmed combat and are considered armed." compset="FraAbility">
    <usesource source="srcRiftsTLPGAE"/>
    <eval phase="Initialize" priority="3001">perform hero.child[wpUnarmed].setfocus



perform focus.delete[WeaponDie.?]

perform focus.assign[WeaponDie.2]

perform focus.assign[Weapon.MegaDamage]</eval>
    </thing>
  <thing id="fcSRAAllMoving" name="All Those Moving Parts" description="’Borgs cannot be healed or treated with medicine when they are hurt. Instead they must be repaired. Fixing a cyborg requires the Repair skill—which is used like the Healing skill, only with no Golden Hour." compset="FraComp">
    <usesource source="srcRiftsTLPGAE"/>
    </thing>
  <thing id="fcSRAFullConver" name="Full Conversion" description="Becoming a Combat Cyborg uses all of a character’s personal Strain ever. They start with only the 6 Strain from the Upgradeable Edge and can only gain more from the Beyond the Limit Edge." compset="FraComp">
    <usesource source="srcRiftsTLPGAE"/>
    <eval phase="Setup" priority="1000">perform hero.assign[Helper.NoStartStrain]</eval>
    </thing>
  <thing id="fcSRAGrayMatter" name="Gray Matter" description="Combat Cyborgs only start with two points (instead of the normal five) for attributes and these may only be spent on Smarts and Spirit. Points from Hindrances and Advances may be used to raise any attribute." compset="FraComp">
    <usesource source="srcRiftsTLPGAE" parent="sepRiftsAE" name="Savage Rifts Core"/>
    <tag group="RaceAbCategoryAE" tag="Comp" name="Comp" abbrev="Comp"/>
    <eval phase="Setup" priority="1000">#resmax[resAttrib] -= 3</eval>
    </thing>
  <thing id="fcSRShaThemselves" name="Shadow of Themselves" description="‘Borgs don’t choose a race, but may increase an attribute one die type or take an Edge (meeting all requirements) to reflect their original race." compset="FraComp" summary="+1 die type to any attribute or gain a bonus edge.">
    <fieldval field="usrCandid1" value="Helper.ShadowOfSelf"/>
    <usesource source="srcRiftsTLPGAE"/>
    <tag group="ChooseSrc1" tag="Thing"/>
    <tag group="User" tag="NoAutoName"/>
    <tag group="ChooseSrc2" tag="Hero"/>
    <eval phase="PreTraits" priority="5000"><![CDATA[var myName as string
var myChoice as string

if (field[usrChosen1].ischosen <> 0) then
  myName = field[usrChosen1].chosen.field[name].text

  if (compare(lowercase(myName),"attribute +1") = 0) then
    field[usrCandid2].text = "component.Attribute & !Attribute.attrSup"
    
    if (field[usrChosen2].ischosen <> 0) then
      perform field[usrChosen2].chosen.field[trtBonus].modify[+,1,"Shadow of Themselves"]
      endif

  elseif (compare(lowercase(myName),"bonus edge") = 0) then
    #resmax[resFraEdges] += 1
	perform assign[Helper.BonusEdge]
  
  endif
  
endif]]>
      <before name="Calc trtFinal"/>
      </eval>
    <eval phase="Render" priority="500" index="2"><![CDATA[var myChoice as string

if (compare(lowercase(field[usrChosen1].chosen.field[name].text),"bonus edge") = 0) then
  myChoice = " (Edge)"
  endif

if (field[usrChosen2].ischosen <> 0) then
  myChoice = " (" & field[usrChosen2].chosen.field[name].text & ")"
  endif

~add the selection to both the livename and shortname fields
field[livename].text = field[name].text & myChoice 
field[shortname].text &= myChoice]]></eval>
    <eval phase="Render" priority="10000" index="3"><![CDATA[
    var myChoice as string
    var header as string
    var chooser as string
    var additem as string
	
    doneif (tagis[Helper.BonusEdge] = 0)

    additem = "Add Edge"
    header = "Shadow of Themselves Edge: "
    header &= hero.child[resFraSel1].field[resSummary].text

    chooser = "!HelperRiftsAE.StartGear"

    ~set up chooser expressions
    hero.child[FrameworkHelper].field[ehFrameEdgeAdd].text = additem
    hero.child[FrameworkHelper].field[ehFrameEdgeExp].text = chooser
    hero.child[FrameworkHelper].field[ehFrameEdgeHdr].text = header]]></eval>
    </thing>
  <thing id="siSRABonusEdge" name="Bonus Edge" description="Combat Cyborgs can choose to gain a bonus edge." compset="Simple" uniqueness="unique">
    <usesource source="srcRiftsTLPGAE"/>
    <tag group="Helper" tag="ShadowOfSelf"/>
    <tag group="Helper" tag="BonusEdge"/>
    </thing>
  <thing id="siSRAAttribute1" name="Attribute +1" description="Combat Cyborgs can choose to improve an attribute by +1 Die Size." compset="Simple" uniqueness="unique">
    <usesource source="srcRiftsTLPGAE"/>
    <tag group="Helper" tag="ShadowOfSelf"/>
    </thing>
  <thing id="fcSRAHeavy" name="Heavy" description="Combat Cyborgs are extra-heavy making it difficult to operate Power Armor and vehicles (including Robot Vehicles) not built for them. Unless noted otherwise, Body Armor and Power Armor must be reinforced, doubling purchase and repair costs. Operating a vehicle not refitted for them is done at a −2 to all Trait checks, including firing vehicular weapons." compset="FraComp" summary=" Body Armor and Power Armor must be reinforced, doubling purchase and repair costs. Operating a vehicle not refitted for them is done at a −2 to all Trait checks, including firing vehicular weapons.">
    <usesource source="srcRiftsTLPGAE"/>
    </thing>
  <thing id="fcSRAInhumanApp" name="Inhuman Appearance" description="Borgs can come across as aloof machines, and suffer −1 to Persuasion rolls when dealing with most beings." compset="FraComp">
    <usesource source="srcRiftsTLPGAE"/>
    <tag group="User" tag="Activation"/>
    </thing>
  <thing id="fcSRALossDex" name="Loss of Dexterity" description="A Combat Cyborg’s systems are designed for combat excellence, not delicate work or even common use. The diminishment in the sense of touch is one of the most glaring issues facing those undergoing bionic augmentation. A ’borg suffers −1 to all Agility rolls and linked skills not directly related to combat." compset="FraComp">
    <usesource source="srcRiftsTLPGAE"/>
    </thing>
  <thing id="fcSRAMalfunctions" name="Malfunctions" description="Cyborgs are subject to the same potential problems all tech items and gear face (see {b}Technical Difficulties{/b} on page 117)." compset="FraComp" summary="Cyborgs are subject to the same potential problems all tech items and gear face.">
    <usesource source="srcRiftsTLPGAE"/>
    </thing>
  <thing id="fcSRASpiritNumb" name="Spiritually Numb" description="Combat Cyborgs can’t take Arcane Background Gifted, Magic, Miracles, or Psionics." compset="FraComp">
    <usesource source="srcRiftsTLPGAE"/>
    </thing>
  <thing id="faSRABioRegen" name="Bio-Regeneration" description="Awakening certain portions of the brain via the Mind Over Matter implants grants the Crazy a natural healing roll once every 24 hours. Permanent injuries may be recovered once all other Wounds are regenerated; treat each injury as an additional Wound for purposes of recovery (the Crazy may try once per week)." compset="FraAbility">
    <usesource source="srcRiftsTLPGAE"/>
    </thing>
  <thing id="faSRAEnhAttrCrazy" name="Enhanced Attributes" description="Crazies gain +2 die types to Agility, Strength, and Vigor; increase Trait maximums an equal amount." compset="FraAbility">
    <usesource source="srcRiftsTLPGAE"/>
    <eval phase="PreTraits" priority="5000">hero.child[attrStr].field[trtMaximum].value += 2
hero.child[attrAgi].field[trtMaximum].value += 2
hero.child[attrVig].field[trtMaximum].value += 2

#traitcreation[attrAgi] += 2
#traitcreation[attrStr] += 2
#traitcreation[attrVig] += 2</eval>
    </thing>
  <thing id="faSRAEnhEndurCraz" name="Enhanced Endurance" description="Crazies begin with the Nerves of Steel Edge, require only half the normal amount of sleep, and gain +4 on checks to resist Fatigue from sleep deprivation." compset="FraAbility" summary="Require only half the normal amount of sleep, and gain +4 on checks to resist Fatigue from sleep deprivation.">
    <usesource source="srcRiftsTLPGAE"/>
    <bootstrap thing="edgNrvStl"></bootstrap>
    </thing>
  <thing id="faSRAEnhSpdCrazy" name="Enhanced Speed" description="Crazies double their base Pace and move twice the normal distance, horizontally and vertically, when Jumping." compset="FraAbility">
    <usesource source="srcRiftsTLPGAE"/>
    <eval phase="Traits" priority="5500"><![CDATA[perform #traitadjust[trPace,*,2,"Crazy"]]]>
      <before name="Derived trtFinal"/>
      <after name="Set Pace"/>
      </eval>
    </thing>
  <thing id="faSRAHeightSense" name="Heightened Senses" description="Crazies gain +2 to Notice and Survival (tracking) rolls. They also ignore two points of Range penalties." compset="FraAbility" summary="+2 to Notice and Survival (tracking) rolls.; Ignore two points of Range penalties.">
    <usesource source="srcRiftsTLPGAE" parent="sepRiftsAE" name="Savage Rifts Core"/>
    <eval phase="PreTraits" priority="5500"><![CDATA[perform #traitroll[skNotice,+,2,"Crazy"]]]></eval>
    </thing>
  <thing id="faSRAPsiMinorCra" name="Minor Psionic" description="Unlocking the brain’s potential grants Arcane Background (Psionics), 10 ISP, three powers, and Psionics d6. Crazies have access to the following powers (Rank permitting): arcane protection*, boost Trait†, confusion, darksight*, detect/conceal arcana*, disguise*, divination, empathy, environmental protection*, farsight*, fear, healing, mind link, relief, warrior’s gift*. Powers marked asterisk (*) have the reduced Power Points cost from the Range (Self) limitation, while those marked dagger (†) have both the Range (Self) and Aspect limitations (see Trappings in Savage Worlds)." compset="FraAbility" summary="Arcane Background (Psionics), 10 ISP, three powers, and Psionics d6.">
    <usesource source="srcRiftsTLPGAE"/>
    <bootstrap thing="skPsionics"></bootstrap>
    <bootstrap thing="edgArcPsiSWADE"></bootstrap>
    <eval phase="PreTraits" priority="5000">~ Psionics at d6
#traitcreation[skPsionics] += 1

hero.childfound[resSkill].field[resSpent].value -= 1</eval>
    </thing>
  <thing id="faSRAQuick" name="Quick" description="Begin with the Quick Edge." compset="FraAbility">
    <usesource source="srcRiftsTLPGAE"/>
    <bootstrap thing="edgQuick"></bootstrap>
    </thing>
  <thing id="faSRAUncReflexes" name="Uncanny Reflexes" description="A Crazy’s reflexes are superhuman, granting them −2 to be hit by any attack they are aware of as well as +2 on Evasion rolls." compset="FraAbility" summary="−2 to be hit by any attack they are aware of as well as +2 on Evasion rolls.">
    <usesource source="srcRiftsTLPGAE"/>
    </thing>
  <thing id="fcSRACyberCrazy" name="Cybernetics" description="Crazies suffer −1 to Psionics per point of Strain." compset="FraComp">
    <usesource source="srcRiftsTLPGAE"/>
    <eval phase="Final" priority="5000">var cyb as number
cyb = #trait[trStrainSRA] - hero.tagcount[Arcane.ReducedCybPen]

hero.childfound[skPsionics].field[trtNetRoll].value -= cyb</eval>
    </thing>
  <thing id="fcSRADistAppCrazy" name="Distinctive Appearance" description="The protrusions from the skull alone mark a Crazy, but most also indulge in tattoos, body piercings, and other means of declaring themselves to the world." compset="FraComp">
    <usesource source="srcRiftsTLPGAE"/>
    </thing>
  <thing id="fcSRAMagicCrazy" name="Magic" description="M.O.M. implants jumble the mind in such a way as to make the use of magic impossible." compset="FraComp">
    <usesource source="srcRiftsTLPGAE"/>
    </thing>
  <thing id="fcSRANeedAction" name="Needs Action" description="Crazies live in a constant state of heightened adrenaline and awareness, even depressed Crazies can&apos;t escape the feeling. During particular slow periods, the GM may call for a Spirit roll. Failure means she becomes irritable or impatient gaining −1 Persuasion, and suffering a −1 to all Trait rolls in this distracted state until a high alert status or action kicks in." compset="FraComp">
    <usesource source="srcRiftsTLPGAE"/>
    </thing>
  <thing id="fcSRAUnstablePsyc" name="Unstable Psyche" description="From the start (and for no added benefit), the character gains Impulsive and a Hindrance rolled on the Psyche Degradation Table. Representing the constant degradation of the Crazy’s psychological state, she gains a Hindrance from the table each time she achieves a new Rank. Simultaneously, her abilities chaotically expand—each new Rank also roll once on the Psionics Hero&apos;s Journey table." compset="FraComp">
    <usesource source="srcRiftsTLPGAE"/>
    </thing>
