    <tag group="ChooseSrc2" tag="Hero"/>
    <tag group="ChooseSrc1" tag="Thing"/>
    <eval phase="Render" priority="500" index="2"><![CDATA[var myChoice as string

if (compare(lowercase(field[usrChosen1].chosen.field[name].text),"bonus edge") = 0) then
  myChoice = " (Edge)"
  endif

if (field[usrChosen2].ischosen <> 0) then
  myChoice = " (" & field[usrChosen2].chosen.field[name].text & ")"
  endif

~add the selection to both the livename and shortname fields
field[livename].text = field[name].text & myChoice 
field[shortname].text &= myChoice]]></eval>
    <eval phase="Render" priority="10000" index="3"><![CDATA[
    var myChoice as string
    var header as string
    var chooser as string
    var additem as string
	
    doneif (tagis[Helper.BonusEdge] = 0)

    additem = "Add Edge"
    header = "DarkHybrid Edge: "
    header &= hero.child[resFrameSelect1].field[resSummary].text

    chooser = "!Helper.StartGear"

    ~set up chooser expressions
    hero.child[FrameworkHelper].field[ehFrameEdgeAdd].text = additem
    hero.child[FrameworkHelper].field[ehFrameEdgeExp].text = chooser
    hero.child[FrameworkHelper].field[ehFrameEdgeHdr].text = header]]></eval>
    <eval phase="PreTraits" priority="5000"><![CDATA[var myName as string
var myChoice as string

if (field[usrChosen1].ischosen <> 0) then
  myName = field[usrChosen1].chosen.field[name].text

  if (compare(lowercase(myName),"attribute +1") = 0) then
    field[usrCandid2].text = "component.Attribute & !Attribute.attrSup"
    
    if (field[usrChosen2].ischosen <> 0) then
      perform field[usrChosen2].chosen.field[trtBonus].modify[+,1,"Dark Hybrid"]
      endif

  elseif (compare(lowercase(myName),"bonus edge") = 0) then
    #resmax[resFraEdges] += 1
	perform assign[Helper.BonusEdge]
  
  endif
  
endif]]>
      <before name="Calc trtFinal"/>
      </eval>
    <eval phase="Setup" priority="1000" index="4">#resmax[resFraEdges] += 1

perform hero.assign[Hero.NoRace]</eval>
    </thing>
