    <eval phase="Setup" priority="1000"><![CDATA[~ doneif nothing selected
doneif (field[usrChosen1].ischosen = 0)

perform field[usrChosen1].chosen.setfocus
perform focus.pushtags[Weapon.?]

var selWt as number
var newWt as number

selWt = focus.field[gearWeight].value
newWt = selWt/3

selWt -= newWt

focus.field[gearWeight].value = selWt

var curReq as number
var newTag as string

curReq = tagvalue[StrReqDie.?]
curReq -= 1
newTag = "StrReqDie." & minimum(12, curReq)

perform focus.delete[StrReqDie.?]
if (curReq >= 2) then 
  perform focus.assignstr[newTag]
  endif]]></eval>
