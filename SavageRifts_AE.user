selWt -= newWt

focus.field[gearWeight].value = selWt

var curReq as number
var newTag as string

curReq = tagvalue[StrReqDie.?]
curReq -= 1
newTag = "StrReqDie." & minimum(12, curReq)

perform focus.delete[StrReqDie.?]
if (curReq >= 2) then 
  perform focus.assignstr[newTag]
  endif]]></eval>
