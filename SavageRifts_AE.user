  <thing id="faSRAWhoDaresWins" name="Who Dares, Wins" description="Commandos can use the Battle, Boating, Driving, Electronics, Healing, Piloting, and Riding skills at no untrained penalty. In addition, they gain a +2 bonus to checks involving parachuting, rappelling, and similar daring maneuvers." compset="FraAbility" summary="+2 bonus to checks involving parachuting, rappelling, and similar daring maneuvers. Also  can use the Battle, Boating, Driving, Electronics, Healing, Piloting, and Riding skills at no untrained penalty.">
    <usesource source="srcRiftsEmpHumAE"/>
    </thing>
  <thing id="edgSRABrawler" name="Brawler" description="His fists hit like hammers or his talons cut like scythes. His body feels like it’s made of stone. Brawlers increase their Toughness by 1 and roll Strength +d4 when hitting with their fists or feet (or claws if they have them). If they already have a damage die from {b}Claws{/b} (page 18), the Martial Artist Edge, etc., increase the damage die type by one instead. \n\nThe Brawler Edge doesn’t make the character’s fists {b}Natural Weapons{/b} (page 104)." compset="Edge" summary="Unarmed +d4+ damage die" uniqueness="unique">
    <usesource source="srcRiftsTLPGAE"/>
    <tag group="MinRank" tag="0"/>
    <tag group="EdgeType" tag="Combat"/>
    <eval phase="Initialize" priority="2100" name="Brawler Adjustment (SRA)"><![CDATA[var newdie as number

perform hero.child[wpSRAUnarmed].setfocus
focus.field[livename].text = "Brawler"
if (focus.tagis[WeaponDie.?] <> 0) then
  newdie = focus.tagvalue[WeaponDie.?] + 1
  perform focus.delete[WeaponDie.?]
  perform focus.assignstr["WeaponDie." & newdie]
else
  perform focus.assign[WeaponDie.2]
  endif
      
~ find any Claws and adjust die type
foreach pick in hero from WeapMelee where "Weapon.IsClaws"
  if (eachpick.tagis[WeaponDie.?] <> 0) then
    newdie = eachpick.tagvalue[WeaponDie.?] + 1
    perform eachpick.delete[WeaponDie.?]
    perform eachpick.assignstr["WeaponDie." & newdie]
    endif
  nexteach]]>
      <before name="Bruiser Adjustment (SRA)"/>
      </eval>
    <eval phase="PreTraits" priority="5000" index="2"><![CDATA[
      perform #traitadjust[trTough,+,1,"Brawler"]]]></eval>
    <exprreq message="Strength d8+ required."><![CDATA[#trait[attrStr] >= 4]]></exprreq>
    <exprreq message="Vigor d8+ required."><![CDATA[#trait[attrVig] >= 4]]></exprreq>
    </thing>
  <thing id="edgSRABruiser" name="Bruiser" description="The fighter increases his Toughness an additional +1, and the damage caused with his fists or claws another die type." compset="Edge" summary="Bonus die to unarmed damage is d8">
    <usesource source="srcRiftsTLPGAE"/>
    <tag group="MinRank" tag="1"/>
    <tag group="EdgeType" tag="Combat"/>
    <eval phase="Initialize" priority="3000" name="Bruiser Adjustment (SRA)"><![CDATA[var newdie as number
perform hero.child[wpSRAUnarmed].setfocus
focus.field[livename].text = "Bruiser"
if (focus.tagis[WeaponDie.?] <> 0) then
  newdie = focus.tagvalue[WeaponDie.?] + 1
  perform focus.delete[WeaponDie.?]
  perform focus.assignstr["WeaponDie." & newdie]
  endif
 
~ find any Claws and adjust die type
foreach pick in hero from WeapMelee where "Weapon.IsClaws"
  if (eachpick.tagis[WeaponDie.?] <> 0) then
    newdie = eachpick.tagvalue[WeaponDie.?] + 1
    perform eachpick.delete[WeaponDie.?]
    perform eachpick.assignstr["WeaponDie." & newdie]
    endif
  nexteach]]></eval>
