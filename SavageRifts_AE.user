    <tag group="ModType" tag="Vehicle"/>
    <eval phase="Effects" priority="5000"><![CDATA[var vehSize as number
var mods as number

vehSize = container.parent.field[vhSize].value

if (vehSize <= 4) then
  mods = 1
elseif (vehSize <= 4) then
  mods = 2
elseif (vehSize <= 4) then
  mods = 3
else
  mods = 4
  endif

field[grModsUsed].text = mods]]></eval>
    </thing>
