  <thing id="rpSRACoreDirecMaj" name="Core Directive (Major)" description="All Constructs are engineered with a primary function, which instills them with purpose. This often functions as the Vow (Major) or Pacifist (Major) Hindrance but may take on other forms (a Major psychology-based Hindrance) per GM&apos;s approval." compset="ExtRacialProp" summary="Requires a Major psychology-based hindrance (Pacifist (Major) or Vow (Major) for example)">
    <usesource source="srcRiftsEmpHumAE"/>
    <eval phase="PreTraits" priority="4000">~set resRaceEdge to selection
#resmax[resRaceHind1] += 1

~Offset Hindrance points
#resmax[resHinder] -= 2</eval>
    <eval phase="Render" priority="10000" index="2"><![CDATA[var header as string
var chooser as string
var additem as string

additem = "Add Hindrance"
header = "Psychological Hindrance: "
header &= hero.child[resRaceHind1].field[resSummary].text
chooser = "(thingid.hinArrogSWADE|thingid.hinDelude|thingid.hinGreedy|thingid.hinImpulsive|thingid.hinJealous|thingid.hinSRAMania|thingid.hinConfide|thingid.hinPacify|thingid.hinSuspicious|thingid.hinVenge|thingid.hinVow)"

~set up chooser expressions for Hindrances
perform hero.child[RacialHelper1].setfocus

focus.field[rhHinAdd].text = additem
focus.field[rhHinExp].text = chooser
focus.field[rhHinHdr].text = header]]></eval>
    <evalrule phase="Validate" priority="1000" message="Requires selection of Major psychology-based hindrance."><![CDATA[var myChoice as string
var myPick as string
var myCount as number

foreach pick in hero from Hindrance where "SpeciesTab.HindSelected1"
  myCount = eachpick.tagis[User.HindMajor]
  if (myCount = 0) then
    @message = eachpick.field[name].text
    @message &= " Hindrance must be Major."
    endif
  nexteach

@valid = myCount]]></evalrule>
    </thing>
