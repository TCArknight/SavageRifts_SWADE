  <thing id="modSRAJetBoostPA" name="Jet Boosters" description="Jump +6&quot; horizontal, +3&quot; vertical" compset="Modificatn" summary="Jump +6&quot; horizontal, +3&quot; vertical">
    <usesource source="srcRiftsTLPGAE"/>
    <tag group="ModType" tag="PowerArmor"/>
    </thing>
