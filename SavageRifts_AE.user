  <thing id="wpSRATail" name="Tail" compset="MeleeSRA">
    <fieldval field="wpSpecial" value="May make one attack per round at no Multi-Action penalty using Athletics."/>
    <usesource source="srcRiftsTLPGAE"/>
    <tag group="WeaponType" tag="MedBlades"/>
    <tag group="Equipment" tag="Natural"/>
