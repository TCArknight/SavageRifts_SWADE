    <usesource source="srcRiftsTLFMAE"/>
    </thing>
  <thing id="abSRASemiAquatic" name="Semi-Aquatic" description="The race is Semi-aquatic (gain Fatigue level every 15 minutes he holds his breath; on reaching Incapacitated, must make Vigor roll every minute or drown; Fatigue recovers one level per 15 minutes back in air)." compset="ExtRaceAbil">
    <usesource source="srcRiftsTLFMAE"/>
    </thing>
  <thing id="rpSRASizeVer" name="Size" description="Vernulians are 750 pounds and rear up 14’ high, on average." compset="ExtRacialProp" uniqueness="unique">
