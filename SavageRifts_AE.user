  <thing id="abSRABite" name="Bite" compset="ExtRaceAbil" summary="Bite attack.">
    <usesource source="srcRiftsTLPGAE"/>
    <tag group="User" tag="Creature" name="Creature" abbrev="Creature"/>
    <bootstrap thing="wpSRABite"></bootstrap>
    <eval phase="Initialize" priority="1000"><![CDATA[perform hero.child[wpSRABite].setfocus

~if we have a weapon die, we must forward it immediately (before it's used)
if (tagis[WeaponDie.?] <> 0) then
  perform focus.pushtags[WeaponDie.?]
  perform focus.pushtags[Helper.DieMult?]
  perform focus.pushtags[Weapon.?]
  endif

~ wpPiercing - abilValue
~ wpReach    - abilValue2
~ wpSpecial - abilText
focus.field[wpPiercing].value = field[abilValue].value
focus.field[wpReach].value = field[abilValue2].value
focus.field[wpSpecial].text = field[abilText].text]]></eval>
    <eval phase="Effects" priority="5000" index="2"><![CDATA[
~customize the Bite pick with the appropriate name
~Note: Do this BEFORE modifying our name below to use an undecorated name.
perform hero.child[wpSRABite].setfocus
focus.field[livename].text = field[name].text

~get our ability text; if we have none, assume "Str"
~Note: This handles the case of a natural weapon with no additional die.
var ability as string
ability = field[abilText].text
if (empty(ability) <> 0) then
  ability = "Str"
  endif

~if we don't have a weapon die, assign the damage text
if (tagis[WeaponDie.?] = 0) then
  focus.field[wpDamage].text = ability
  endif

~if we have a value, assign to piercing
if (field[abilValue].value <> 0) then
  focus.field[wpPiercing].value = field[abilValue].value
  endif

~if our live name is currently empty, use our default name
if (field[livename].isempty <> 0) then
  field[livename].text = field[name].text
  endif

~update our name appropriately for display
if (tagis[WeaponDie.?] <> 0) then
  var die as number
  var dienum as number

  die = tagvalue[WeaponDie.?] * 2
  dienum = tagvalue[Helper.DieMult?]

  field[livename].text &= " (Str+"
  if (dienum  >= 2) then
    field[livename].text &= dienum  & "d" & die
  else
    field[livename].text &= "d" & die
    endif

else
  field[livename].text &= " (" & ability
  endif

if (tagis[Weapon.?] <> 0) then
  field[livename].text &= ", "
  field[livename].text &= tagnames[Weapon.?,", "]
  endif

if (field[abilValue].value <> 0) then
  field[livename].text &= ", AP: " & field[abilValue].value
  endif

field[livename].text &= ")"]]></eval>
    </thing>
