  <thing id="siSRABreedFast" name="Fast" compset="Simple">
    <usesource source="srcRiftsTLPGAE"/>
    <tag group="BreedAdvAE" tag="Fast"/>
    <tag group="Helper" tag="Bootstrap"/>
    <eval phase="PreTraits" priority="6000"><![CDATA[var myCount as number

myCount = hero.tagcount[BreedAdvAE.Fast]

doneif (myCount = 0)

if (hero.tagis[source.RunningDie] <> 0) then
  perform #traitadjust[trRunning,+,2,"Breed Advantage (Fast)"]
  endif]]></eval>
    <eval phase="Traits" priority="5500" index="2"><![CDATA[var myCount as number

myCount = hero.tagcount[BreedAdvAE.Fast]

doneif (myCount = 0)

perform #traitadjust[trPace,+,2,"Breed Advantage (Fast)"]]]>
      <before name="Derived trtFinal"/>
      <after name="Set Pace"/>
      </eval>
    </thing>
  <thing id="edgSRABreedAdv" name="Breed Advantage" description="The various breeds of canine provide distinct genetic advantages for individual Dog Boys. Some are fast, others strong, still others astoundingly good trackers.\n\nThis Edge may be taken more than once to represent a breed more fully expressing itself over time, but only once per Rank. The GM has final approval over each choice to ensure it makes sense. Each time the Edge is taken, choose one of the following advantages:\n\nAgile, Strong or Rugged - Agility, Strength or Vigor increases by 1 rank\nFast - Pace +2, Running die up a type. \nTough - Toughness +2\nTracking - +2 on all Tracking rolls\nWater - d6 (or +1 die type increase) in Swimming skill" compset="Edge">
    <fieldval field="usrCandid1" value="BreedAdvAE.?"/>
    <usesource source="srcRiftsTLPGAE" parent="sepRiftsAE" name="Savage Rifts Core"/>
    <tag group="Helper" tag="BreedAdvMain" name="BreedAdvMain" abbrev="BreedAdvMain"/>
    <tag group="MinRank" tag="0" name="Novice" abbrev="Novice"/>
    <tag group="ChooseSrc1" tag="Thing" name="All Things" abbrev="All Things"/>
    <tag group="EdgeType" tag="Racial" name="Racial" abbrev="Racial"/>
    <eval phase="Initialize" priority="5000"><![CDATA[
	~get name from usrChosen1
	var myChoice as string

	if (field[usrChosen1].ischosen <> 0) then
	  myChoice = "BreedAdvAE." & field[usrChosen1].chosen.field[name].text
	  perform assignstr[myChoice]
	  perform forward[BreedAdvAE.?]
	  endif]]></eval>
    <eval phase="Initialize" priority="6000" index="2"><![CDATA[
	if (tagis[BreedAdvAE.Agile] = 1) then
	  perform state.thing[edgSRABreedAdv].amendthing[description,"Agility increases by 1 rank"]
	elseif (tagis[BreedAdvAE.Strong] = 1) then
	  perform state.thing[edgSRABreedAdv].amendthing[description,"Strength increases by 1 rank"]
	elseif (tagis[BreedAdvAE.Rugged] = 1) then
	  perform state.thing[edgSRABreedAdv].amendthing[description,"Vigor increases by 1 rank"]
	elseif (tagis[BreedAdvAE.Fast] = 1) then
	  perform state.thing[edgSRABreedAdv].amendthing[description,"Pace +2, Running die increases a type"]
	elseif (tagis[BreedAdvAE.Tough] = 1) then
	  perform state.thing[edgSRABreedAdv].amendthing[description,"+2 Toughness"]
	elseif (tagis[BreedAdvAE.Tracker] = 1) then
	  perform state.thing[edgSRABreedAdv].amendthing[description,"+2 on all Tracking rolls"]
	elseif (tagis[BreedAdvAE.Water] = 1) then
	  perform state.thing[edgSRABreedAdv].amendthing[description,"d6 (or +1 die type increase) in Swimming skill"]
	  endif]]></eval>
    <exprreq message="Not required race."><![CDATA[hero.tagis[Helper.BreedValid] <> 0]]></exprreq>
    </thing>
  <thing id="edgSRAContHunger" name="Controlled Hunger" description="Psi-Stalkers, especially those who leave their tribe, can learn to master their hunger. They need to feed less often, and need far less to feel satisfied. \n\nA Psi-Stalker with this Edge needs to consume only 30 Power Points every two weeks to avoid Fatigue levels, and can drain friendly beings without violence." compset="Edge" summary="Only require 30 Power Points every two weeks to avoid Fatigue." uniqueness="unique">
    <usesource source="srcRiftsTLPGAE" parent="sepRiftsAE" name="Savage Rifts Core"/>
    <tag group="MinRank" tag="0" name="Novice" abbrev="Novice"/>
    <tag group="EdgeType" tag="Racial" name="Racial" abbrev="Racial"/>
    <pickreq thing="racSRAPsiStalker"/>
    <exprreq message="Spirit d6+ required."><![CDATA[#trait[attrSpi] >= 3]]></exprreq>
    <exprreq message="Vigor d8+ required."><![CDATA[#trait[attrVig] >= 4]]></exprreq>
    </thing>
  <thing id="edgSRAHunter" name="Hunter" description="Some Dog Boys and Psi-Stalkers focus on hunting specific prey. Some choose magic-users (characters with a PPE-based Arcane Background). Others choose supernatural creatures (demons, dragons, vampires, etc.). \n\nHunters gain a +2 on Notice and Survival rolls against their preferred targets, and deal d8 (instead of d6) bonus damage when rolling a raise on an attack. This Edge can only be taken once." compset="Edge" summary="+2 Notice and Tracking rolls against specified target." uniqueness="unique">
    <usesource source="srcRiftsTLPGAE" parent="sepRiftsAE" name="Savage Rifts Core"/>
    <tag group="EdgeType" tag="Racial"/>
    <tag group="MinRank" tag="1" name="Seasoned" abbrev="Seasoned"/>
    <tag group="User" tag="Activation" name="Activation" abbrev="Activation"/>
    <tag group="User" tag="NeedDomain" name="NeedDomain" abbrev="NeedDomain"/>
    <tag group="DomainTerm" tag="Creature" name="Creature Type" abbrev="Crea"/>
    <eval phase="PreTraits" priority="5000"><![CDATA[
      if (field[abilActive].value <> 0) then
        perform #traitroll[skNotice,+,2,"Hunter"]
        perform #traitroll[skTracking,+,2,"Hunter"]
        endif]]></eval>
    <exprreq message="Spirit d6+ required."><![CDATA[#trait[attrSpi] >= 3]]></exprreq>
    <exprreq message="Fighting d8+ required."><![CDATA[#traitfound[skFighting] >= 4]]></exprreq>
    <exprreq message="Dog Boy or Psi-Stalker race required."><![CDATA[(hero.tagis[Race.racSRAPsiStalker] + hero.tagis[Race.racSRADogBoy] ) <> 0]]></exprreq>
    <exprreq message="Vigor d8+ required."><![CDATA[#trait[attrVig] >= 4]]></exprreq>
    </thing>
  <thing id="edgSRAPsyBloodHnd" name="Psychic Bloodhound" description="A few Psi-Stalkers and Dog Boys become quite refined in their senses. Their Psychic Sense gains the effect of the Exalted Detect Arcana power modifier." compset="Edge" summary="Psychic Sense gains the effect of the Exalted Detect Arcana power modifier." uniqueness="unique">
    <usesource source="srcRiftsTLPGAE" parent="sepRiftsAE" name="Savage Rifts Core"/>
    <tag group="EdgeType" tag="Racial"/>
    <tag group="MinRank" tag="2"/>
    <tag group="User" tag="Activation" name="Activation" abbrev="Activation"/>
    <tag group="User" tag="NeedDomain" name="NeedDomain" abbrev="NeedDomain"/>
    <tag group="DomainTerm" tag="Creature" name="Creature Type" abbrev="Crea"/>
    <exprreq message="Spirit d6+ required."><![CDATA[#trait[attrSpi] >= 3]]></exprreq>
    <exprreq message="Notice d10+ required."><![CDATA[#traitfound[skNotice] >= 5]]></exprreq>
    <exprreq message="Dog Boy or Psi-Stalker race required."><![CDATA[(hero.tagis[Race.racSRAPsiStalker] + hero.tagis[Race.racSRADogBoy] ) <> 0]]></exprreq>
    </thing>
  <thing id="edgSRAMajorPsi" name="Major Psionic" description="A greater control of mind, body, and spirit combined with the overwhelming power available on Rifts Earth means this hero can do much more with her inner strength. Major Psionics double the ISP gained from the Arcane Background (Psionics) and Power Points Edges.\n\nBefore attempting a Psionics skill roll, Major and Master Psionics may spend 2 ISP to gain +1 to the roll, or 4 ISP for +2." compset="Edge" summary="Double ISP. Before attempting a Psionics skill roll, may spend 2 ISP to gain +1 to the roll, or 4 ISP for +2.">
    <usesource source="srcRiftsTLPGAE"/>
    <tag group="MinRank" tag="1"/>
    <tag group="EdgeType" tag="Power"/>
    <eval phase="PreTraits" priority="5100">perform hero.childfound[arcPsionicSWADE].setfocus
doneif (state.isfocus = 0)

focus.field[trkMax].value *= 2</eval>
    </thing>
  <thing id="edgSRAMasterPsi" name="Master Psionic" description="The psychic has unlocked the penultimate powers of the mind and spirit—the Master Psionic gains access to the Mega Power Modifiers for all known psionics powers. Those with mind link may take mind reading. They also add elemental manipulation to their power list applying either an ectoplasm (equivalent to earth), electrokinetic (air), hydrokinetic (water), or pyrokinetic (fire) Trapping to the power. Master Psionics may either apply the chosen Trapping to their powers or the standard telekinetic force Trapping. Mind Melters who take elemental manipulation may freely choose among all of these Trappings or the standard telekinetic force Trapping when activating powers." compset="Edge" summary="Gain access to Mega Power modifiers. Gain access to {/i}elemental manipulation{/i} power.">
    <usesource source="srcRiftsTLPGAE"/>
    <tag group="MinRank" tag="0"/>
    <tag group="EdgeType" tag="Background"/>
    </thing>
  <thing id="edgSRABrawny" name="Brawny" description="Your bruiser is very large or very fit. Her Toughness increases by 1 and she treats her Strength as one die type higher when determining {b}Encumbrance{/b} (page 67) and {b}Minimum Strength{/b} to use armor, weapons, and equipment without a penalty (page 66)." compset="Edge" summary="+1 Tough; Str Die +1 for Enc">
    <usesource source="srcRiftsTLPGAE"/>
    <tag group="MinRank" tag="0"/>
    <tag group="EdgeType" tag="Background"/>
    <eval phase="PreTraits" priority="5000"><![CDATA[perform #traitadjust[trTough,+,1,"Brawny"]
herofield[acLoadBonus].value += 1]]></eval>
    <pickreq thing="hinObeseSWADE" ispreclude="yes"/>
    <exprreq message="Vigor d6+ required"><![CDATA[#trait[attrVig] >= 3]]></exprreq>
    <exprreq message="Strength d6+ required"><![CDATA[#trait[attrStr] >= 3]]></exprreq>
    </thing>
  <thing id="edgSRARich" name="Rich" description="Unless the hero is planning to remain in one location for a serious length of time, annual salary isn’t part of the wealth equation. She’s managed to accumulate a bit of money, but more importantly, extra gear. She begins with 15,000 credits and gets two rolls on the Body Armor, Close Combat Weapons, Cybernetics, Enchanted Items &amp; Mystic Gadgets, or Ranged Combat Weapons Tables. The hero may give up the two rolls to instead own one personal weapon, body armor, or adventuring gear item of her choice." compset="Edge" summary="Begin with 15000 credits; 2 additional HJ rolls (or 1 one personal weapon, body armor, or adventuring gear item of choice)" uniqueness="unique">
    <usesource source="srcRiftsTLPGAE"/>
    <tag group="MinRank" tag="0"/>
    <tag group="EdgeType" tag="Background"/>
    <eval phase="Setup" priority="5000">trustme
herofield[acCashCfg].value = 15000</eval>
    </thing>
  <thing id="edgSRARich2" name="Filthy Rich" description="This hero made some major scores before the current adventure. She has a total of 25,000 credits saved up and gets four total rolls on the above-listed tables. She may give up two rolls to instead own one personal weapon, body armor, or adventuring gear item of choice, or three rolls for one non-robot vehicle of choice." compset="Edge" summary="Begin with 15000 credits; 4 additional HJ rolls (or exchange rolls for specific items)" uniqueness="unique">
    <usesource source="srcRiftsTLPGAE"/>
    <tag group="MinRank" tag="0"/>
    <tag group="EdgeType" tag="Background"/>
    <eval phase="Setup" priority="5000">trustme
herofield[acCashCfg].value = 25000</eval>
    <pickreq thing="edgSRARich"/>
    </thing>
  <thing id="edgSRASepFaction" name="– Faction Edges –" compset="Edge" isshowonly="yes">
    <usesource source="srcRiftsTLPGAE"/>
    <tag group="EdgeType" tag="Racial"/>
    </thing>
  <thing id="edgSRAFlameBurst" name="Flame Blast" description="For one ISP the Burster’s Flame Bolt affects a Small Blast Template, two ISP for a Medium one, or three ISP for a Large one. Alternately, she may spend one ISP to use the Cone Template at no Range. Unless otherwise stated, enhancements to Flame Bolt apply when using this Edge as well." compset="Edge" summary="For additional ISP (1/2/3) Flame Bolt may affect SBT/MBT/LBT or for 1 ISP may use a Cone Template at no range.">
    <usesource source="srcRiftsTLPGAE"/>
    <tag group="MinRank" tag="1"/>
    <tag group="EdgeType" tag="Iconic" name="Iconic"/>
    <pickreq thing="grpSRABurster"/>
    </thing>
  <thing id="edgSRAImpFieryAur" name="Improved Fiery Aura" description="The Burster’s Fiery Aura damage increases to 3d8, and she may choose to affect a Small Blast Template centered on herself." compset="Edge">
    <usesource source="srcRiftsTLPGAE"/>
    <tag group="MinRank" tag="1"/>
    <tag group="EdgeType" tag="Iconic" name="Iconic"/>
    <pickreq thing="grpSRABurster"/>
    </thing>
  <thing id="edgSRAImpFireMast" name="Improved Fire Mastery" description="Many Bursters grow in their absolute control over fire and heat, improving their natural Fire Mastery in a number of ways.\n\nA Burster with this Edge increases the Range of his Fire Mastery to Smarts × 2, and the Strength of the fire effects to d12." compset="Edge" summary="The Range of Fire Mastery increases to Smarts × 2, and the Strength of the fire effects to d12.">
    <usesource source="srcRiftsTLPGAE"/>
    <tag group="MinRank" tag="1"/>
    <tag group="EdgeType" tag="Iconic" name="Iconic"/>
    <pickreq thing="grpSRABurster"/>
    </thing>
  <thing id="edgSRAImpFlaBolt" name="Improved Flame Bolt" description="For some Bursters, more destructive power is the summation of their true self. Improve the Burster’s 2 ISP Flame Bolt damage to 5d6 Mega Damage (6d6 on a raise) with Range Smarts × 3. Flame Bolt remains the same in all other aspects." compset="Edge" summary="2 ISP Flame Bolt damage to 5d6 Mega Damage (6d6 on a raise) with Range Smarts × 3. Flame Bolt remains the same in all other aspects.">
    <usesource source="srcRiftsTLPGAE"/>
    <tag group="MinRank" tag="0"/>
    <tag group="EdgeType" tag="Iconic" name="Iconic"/>
    <pickreq thing="grpSRABurster"/>
    </thing>
  <thing id="edgSRARapFlameBol" name="Rapid Flame Bolt" description="For one ISP the Burster’s Flame Bolt power (along with any enhancements from other Edges or Modifiers) becomes Rate of Fire 2." compset="Edge">
    <usesource source="srcRiftsTLPGAE"/>
    <tag group="MinRank" tag="2"/>
    <tag group="EdgeType" tag="Iconic" name="Iconic"/>
    <pickreq thing="grpSRABurster"/>
    </thing>
  <thing id="edgSRAReadFlame" name="Read The Flame" description="Bursters with this Edge can make a Notice check to read and understand a fire, or the site of one. With a success, she can tell how it started and how long it burned. With a raise, she can sense what kind of being, if any, started the fire, and any other interesting details the GM might wish to share, such as the fire’s purpose, if any." compset="Edge">
    <usesource source="srcRiftsTLPGAE"/>
    <tag group="MinRank" tag="0"/>
    <tag group="EdgeType" tag="Iconic" name="Iconic"/>
    <pickreq thing="grpSRABurster"/>
    </thing>
  <thing id="edgSRABeyondLimit" name="Beyond The Limit" description="There are those so obsessed with cybernetic upgrades that they sacrifice sanity for superiority. Having taken all the cybernetics their humanity should be able to bear, they somehow manage to shove even more into their body, and damn the consequences.\n\nIncrease maximum Strain by +6, but the player must select one of the Unstable Psyche Hindrances (see Crazies, page 22)." compset="Edge" summary="Increase maximum Strain by +6, but must select one of the Unstable Psyche Hindrances">
    <usesource source="srcRiftsTLPGAE"/>
    <tag group="MinRank" tag="2"/>
    <tag group="EdgeType" tag="Iconic" name="Iconic"/>
    <exprreq message="Combat Cyborg or MARS required."><![CDATA[hero.tagis[Group.grpSRACyborgCbt] + hero.tagis[Helper.IsMARS] <> 0]]></exprreq>
    </thing>
  <thing id="edgSRAUpgrade" name="Upgrade" description="For those who embrace cybernetics as a part of their being (by choice or necessity), the desire to upgrade is often an all-encompassing goal on its own. \n\nTaking this Edge—which may only be selected once per Rank—means the cybernetic hero manages to achieve the necessary resources and opportunity to have a new piece of cybernetic technology installed, or have one upgraded to a higher degree. Though the GM has final say on what is and is not appropriate, anything in the Cybernetics section is fair game otherwise." compset="Edge" summary="Install or upgrade a piece of cybernetic technology.">
    <usesource source="srcRiftsTLPGAE"/>
    <tag group="MinRank" tag="1"/>
    <tag group="EdgeType" tag="Iconic" name="Iconic"/>
    <exprreq message="Combat Cyborg or MARS required."><![CDATA[hero.tagis[Group.grpSRACyborgCbt] + hero.tagis[Helper.IsMARS] <> 0]]></exprreq>
    </thing>
  <thing id="edgSRAGunNut" name="Gun Nut" description="For many Crazies, the gun is a tool of unrestrained violence and a key partner in the ballet of rage that overcomes them.\n\nWhile Losing It a Crazy may add +2 to her Shooting attacks and resulting damage rolls with pistols against adjacent melee opponents (see {b}Ranged Weapons In Melee{/b} in {i}Savage Worlds{/i})." compset="Edge">
    <usesource source="srcRiftsTLPGAE"/>
    <tag group="MinRank" tag="0"/>
    <tag group="EdgeType" tag="Iconic" name="Iconic"/>
    <pickreq thing="grpSRACrazy"/>
    </thing>
  <thing id="edgSRAOffHandle" name="Off The Handle" description="The Crazy can start Losing It as a free action, and has an easier time coming out of it. She doesn’t suffer the usual −2 penalty to her Smarts roll when trying to recover and with a raise halves her penalty (rounding up) to all Trait rolls while Getting It Together." compset="Edge" summary="Losing It as a free action. doesn’t suffer the usual −2 penalty to her Smarts roll when trying to recover and with a raise halves her penalty (rounding up) to all Trait rolls while Getting It Together.">
    <usesource source="srcRiftsTLPGAE"/>
    <tag group="MinRank" tag="1"/>
    <tag group="EdgeType" tag="Iconic" name="Iconic"/>
    <pickreq thing="grpSRACrazy"/>
    </thing>
  <thing id="edgSRAGymnastMast" name="Gymnastic Mastery" description="Some become preternaturally gifted in matters of motion and space. A character with this Edge adds +2 to her Athletics rolls when Jumping. She can ascend at her Athletics die per round (instead of half) and gains an extra +2 to Parry when using the Defend maneuver. \n\nFinally, the hero can check Agility (with the +2 from Acrobat) to reduce effective falling distance by 10 feet, or 20 feet on a raise. Falls of over 20 yards are a −2 penalty." compset="Edge">
    <usesource source="srcRiftsTLPGAE"/>
    <tag group="MinRank" tag="2"/>
    <tag group="EdgeType" tag="Iconic" name="Iconic"/>
    <exprreq message="Crazy or Juicer required."><![CDATA[hero.tagis[Group.grpSRACrazy] + hero.tagis[Group.grpSRAJuicer] <> 0]]></exprreq>
    </thing>
  <thing id="edgSRASplitSecond" name="Split The Seconds" description="Juicers and Crazies often find themselves living between the ticks of the clock. This Edge allows them to ignore 2 points of Multi- Action penalties each turn (they can perform two actions at no penalty or three at −2 each)." compset="Edge" summary="Ignore 2 points of Multi- Action penalties each turn.">
    <usesource source="srcRiftsTLPGAE"/>
    <tag group="MinRank" tag="1"/>
    <tag group="EdgeType" tag="Iconic" name="Iconic"/>
    <exprreq message="Crazy or Juicer required."><![CDATA[hero.tagis[Group.grpSRACrazy] + hero.tagis[Group.grpSRAJuicer] <> 0]]></exprreq>
    </thing>
  <thing id="edgSRASublimChaos" name="Sublime Chaos" description="The tumultuous storm of neuroses, psychoses, and other issues crashing about in a Crazy’s head sometimes swirl into a beautiful rainbow of total disconnection from reality. The Crazy with this Edge understands how to ride the waves of her insanity like a surfer masters those of the sea. The greater part of her mind is simply elsewhere, leaving behind a nearly machinelike entity that gets the job done. \n\nWhile Losing It the Crazy is not Fatigued by Berserk, and is able to perform actions not normally allowed, including using Psionics. The Crazy&apos;s time spent Getting It Together is halved." compset="Edge" summary="While Losing It the Crazy is not Fatigued by Berserk, and is able to perform actions not normally allowed, including using Psionics. The Crazy&apos;s time spent Getting It Together is halved.">
    <usesource source="srcRiftsTLPGAE"/>
    <tag group="MinRank" tag="2"/>
    <tag group="EdgeType" tag="Iconic" name="Iconic"/>
    <pickreq thing="grpSRACrazy"/>
    </thing>
  <thing id="edgSRACybKinAwa" name="Cyberkinetic Awareness" description="As an action, the Cyber-Knight can enhance his Cyberkinetic Combat abilities by stretching out his senses to understand all of the electronic technology he’s engaged with. Enemies within a distance of Smarts ×2 are −2 on all Trait rolls against him where technology is involved; includes Notice checks and opposed rolls. This ability lasts until the end of the encounter." compset="Edge" summary="As an action, until the end of the encounter, enemies within a distance of Smarts ×2 are −2 on all Trait rolls against you where technology is involved">
    <usesource source="srcRiftsTLPGAE"/>
    <tag group="MinRank" tag="1"/>
    <tag group="EdgeType" tag="Iconic" name="Iconic"/>
    <pickreq thing="grpSRACyberKnight"/>
    </thing>
  <thing id="edgSRAImpCybKinAw" name="Improved Cyberkinetic Awareness" description="Once Cyberkinetic Awareness has been activated, all rolls against the Cyber-Knight where electronic technology is involved suffer a −4 penalty." compset="Edge" summary="When using Cyberkinetic Awareness, penalty is -4.">
    <usesource source="srcRiftsTLPGAE"/>
    <tag group="MinRank" tag="2"/>
    <tag group="EdgeType" tag="Iconic" name="Iconic"/>
    <pickreq thing="edgSRACybKinAwa"/>
    </thing>
  <thing id="edgSRACybkinDen" name="Cyberkinetic Denial" description="Once Cyberkinetic Awareness is activated, the Cyber-Knight gets a free reroll on all associated opposed rolls." compset="Edge">
    <usesource source="srcRiftsTLPGAE"/>
    <tag group="MinRank" tag="3"/>
    <tag group="EdgeType" tag="Iconic" name="Iconic"/>
    <pickreq thing="edgSRAImpCybKinAw"/>
    </thing>
  <thing id="edgSRAImpCybArmor" name="Improved Cyber-Armor" description="The protection of the Cyber-Armor is enhanced to +4 Toughness total." compset="Edge">
    <usesource source="srcRiftsTLPGAE"/>
    <tag group="MinRank" tag="1"/>
    <tag group="EdgeType" tag="Iconic" name="Iconic"/>
    <eval phase="Effects" priority="5000">hero.childfound[faSRACyberArmorCK].field[frabValue].value += 2</eval>
    <pickreq thing="grpSRACyberKnight"/>
    </thing>
  <thing id="edgSRAMasCybArmor" name="Master Cyber-Armor" description="The protection of the Cyber-Armor is enhanced to +6 Toughness total. When activated while wearing Cyber-Knight medium or heavy EBA suits, the suit&apos;s Armor bonus is MDC Armor." compset="Edge">
    <usesource source="srcRiftsTLPGAE"/>
    <tag group="MinRank" tag="3"/>
    <tag group="EdgeType" tag="Iconic" name="Iconic"/>
    <eval phase="Effects" priority="5000">hero.childfound[faSRACyberArmorCK].field[frabValue].value += 2</eval>
    <pickreq thing="edgSRAImpCybArmor"/>
    </thing>
  <thing id="edgSRAImpPsiSword" name="Improved Psi-Sword" description="The Psi-Sword is enhanced dealing Str × 2 + Spirit × 2, with AP equal to Psionics × 2. If split, each blade&apos;s AP is equal to Psionics. Cannot be taken at character creation." compset="Edge">
    <usesource source="srcRiftsTLPGAE"/>
    <tag group="MinRank" tag="1"/>
    <tag group="EdgeType" tag="Iconic" name="Iconic"/>
    <pickreq thing="grpSRACyberKnight"/>
    </thing>
  <thing id="edgSRAMasPsiSword" name="Master Psi-Sword" description="The Cyber-Knight can channel her inner strength into her Psi-Sword. Before rolling an attack, the player declares how many ISP to spend on damage, up to 6 points. Each point adds +2 to the damage (up to a maximum of +12). If using the split blade option, each blade gains +1 damage per ISP spent (to a maximum of +6 each)." compset="Edge">
    <usesource source="srcRiftsTLPGAE"/>
    <tag group="MinRank" tag="3"/>
    <tag group="EdgeType" tag="Iconic" name="Iconic"/>
    <pickreq thing="edgSRAImpPsiSword"/>
    </thing>
  <thing id="edgSRAPsiShield" name="Psi-Shield" description="Cyber-Knights with this Edge can summon a Psi-Shield, granting +3 Parry and a −4 Cover penalty to ranged strikes against her, see {b}Shields{/b} in {i}Savage Worlds{/i}. The Psi-Shield costs no ISP and can be summoned as a free action. While activated, the Psi-Shield can be used to bash for Str+2d4 damage, and provides the wielder&apos;s Spirit in MDC Armor if an enemy attempts to shoot through it." compset="Edge" summary="Summon Psi-Shield as a free action.">
    <usesource source="srcRiftsTLPGAE"/>
    <tag group="MinRank" tag="1"/>
    <tag group="EdgeType" tag="Iconic" name="Iconic"/>
    <tag group="User" tag="Activation"/>
    <bootstrap thing="shdSRAPsiShield">
      <containerreq phase="Initialize" priority="900">hero#Helper.PsiShieldActive</containerreq>
      <autotag group="ArmorSpecial" tag="MegaDamage"/>
      </bootstrap>
    <eval phase="Initialize" priority="500"><![CDATA[if (field[abilActive].value <> 0) then
  perform hero.assign[Helper.PsiShieldActive]
  endif]]></eval>
    <eval phase="Final" priority="1000" index="2">hero.childfound[shdSRAPsiShield].field[defDefense].value += #trait[attrSpi]*2</eval>
    <pickreq thing="grpSRACyberKnight"/>
    </thing>
  <thing id="edgSRADraFstRegen" name="Dragon Fast Regeneration" description="A dragon’s regenerative abilities can improve over time, allowing him to make a natural healing roll every round. He also gains +2 to recover from being Shaken and Stunned." compset="Edge" summary="natural healing roll every round. Also gains +2 to recover from being Shaken and Stunned.">
    <tag group="EdgeType" tag="Iconic"/>
    <tag group="MinRank" tag="2"/>
    <pickreq thing="racSRADragonHatch"/>
    </thing>
  <thing id="edgSRASepIconic" name="– Iconic Edges –" compset="Edge" isshowonly="yes">
    <usesource source="srcRiftsTLPGAE"/>
    <tag group="EdgeType" tag="Iconic"/>
    </thing>
  <thing id="edgSRADraFlaScale" name="Dragon Flaming Scales" description="As an action the Hatchling sets its scales aflame per the damage field power but dealing 3d6 damage for their Spirit in rounds. Targets affected may ignite (see Fire in Savage Worlds). The Hatchling must wait 10 minutes before reactivating the power." compset="Edge">
    <tag group="EdgeType" tag="Iconic"/>
    <tag group="MinRank" tag="1"/>
    <pickreq thing="grpSRADraHatFlame"/>
    </thing>
  <thing id="edgSRADraLowLight" name="Dragon Low Light Vision" description="Dragons can develop strong night vision, which enables them to ignore Dim and Dark Illumination penalties." compset="Edge">
    <tag group="EdgeType" tag="Iconic"/>
    <tag group="MinRank" tag="0"/>
    <pickreq thing="racSRADragonHatch"/>
    </thing>
  <thing id="edgSRADraMetamor" name="Dragon Metamorphosis" description="The dragon may change his shape and size, appearing as any living creature from the size of a house cat to anything his natural size and weight. He can maintain any such form indefinitely, even while sleeping. The dragon gains no special properties of what he mimics, only the size and appearance." compset="Edge">
    <tag group="EdgeType" tag="Iconic"/>
    <tag group="MinRank" tag="2"/>
    <pickreq thing="racSRADragonHatch"/>
    </thing>
  <thing id="edgSRADraGrowLarg" name="Dragon Grows Larger" description="As they grow, dragons gain in size and strength; it gains a die type in Strength and grows to Size 7 (Large), gaining +4 Armor and +1 Toughness for the Size increase." compset="Edge">
    <tag group="EdgeType" tag="Iconic"/>
    <tag group="MinRank" tag="2"/>
    <pickreq thing="racSRADragonHatch"/>
    </thing>
  <thing id="edgSRADraGrowHuge" name="Dragon Grows Huge" description="Upon taking this Edge the dragon gains a die type in Strength and grows to Size 8 (Huge), gaining +1 Toughness for the Size increase. The dragon’s hide thickens providing an additional +4 MDC Armor and +2 Toughness, his Claw/Bite damage increases to Str+3d4, AP equal to Size × 2, the damage of any breath attacks increases by one die type, and he applies a −2 penalty to opponents’ Fear checks. Note that his Size category changes from Large to Huge and he can take a fifth Wound." compset="Edge">
    <tag group="EdgeType" tag="Iconic"/>
    <tag group="MinRank" tag="3"/>
    <pickreq thing="racSRADragonHatch"/>
    <pickreq thing="edgSRADraGrowLarg"/>
    </thing>
  <thing id="edgSRADraTeleport" name="Dragon Teleportation" description="Dragons can learn to naturally teleport as an action by checking Spirit and spending PPE. Double the effect of the Range modifier, but can&apos;t be combined with any others. At Veteran, when on a nexus point he may teleport to a previously visited dimension." compset="Edge">
    <tag group="EdgeType" tag="Iconic"/>
    <tag group="MinRank" tag="1"/>
    <pickreq thing="grpSRADraHatFlame"/>
    </thing>
  <thing id="edgSRABurnBright" name="Burn Brighter" description="Every drop of life is precious to a Juicer. She never wants to see it wasted. With this Edge, any time a Burn Die rolls a 1 the player still spends her Burn, but adds 6 to the Trait roll!" compset="Edge">
    <tag group="EdgeType" tag="Iconic"/>
    <tag group="MinRank" tag="0"/>
    <pickreq thing="grpSRAJuicer"/>
    </thing>
  <thing id="edgSRABurnGlory" name="Burn For Glory" description="For the rare Juicer who lives long enough to become Legendary, the possibilities are truly spectacular for what she can do with her remaining days. \n\nA Juicer with this Edge may spend a point of Burn (no roll) to double the result of any Trait or damage roll!" compset="Edge">
    <tag group="EdgeType" tag="Iconic"/>
    <tag group="MinRank" tag="4"/>
    <pickreq thing="grpSRAJuicer"/>
    </thing>
  <thing id="edgSRABurnFaster" name="Burn Faster" description="There never seems to be quite enough time left, so the Juicer just has to go faster to get it all done. She can spend a Burn to ignore 4 points of Multi-Action penalties each turn when her Action Card is a face card (she can perform three actions at no penalty). The effect lasts for 5 rounds." compset="Edge">
    <tag group="EdgeType" tag="Iconic"/>
    <tag group="MinRank" tag="3"/>
    <pickreq thing="grpSRAJuicer"/>
    <pickreq thing="edgSRASplitSecond"/>
    </thing>
  <thing id="edgSRABurnPastPai" name="Burn Past The Pain" description="He doesn’t have time to bleed or feel pain. The Juicer can spend a Burn to ignore all Wound penalties for the remainder of a combat, including Wound penalties to Vigor rolls if he is Incapacitated during the combat (as per the Hard to Kill Edge)." compset="Edge">
    <tag group="EdgeType" tag="Iconic"/>
    <tag group="MinRank" tag="1"/>
    <pickreq thing="grpSRAJuicer"/>
    </thing>
  <thing id="edgSRABurnTomorro" name="Burn For Tomorrow" description="Death remains close on the Juicer’s heels, but he refuses to acknowledge it until his time has truly come. When rolling on the Death &amp; Defeat Table, page 117, he may add his Burn Die to the d20 roll." compset="Edge">
    <tag group="EdgeType" tag="Iconic"/>
    <tag group="MinRank" tag="2"/>
    <pickreq thing="edgSRABurnPastPai"/>
    </thing>
  <thing id="edgSRAFreshJuice" name="Freshly Juiced" description="For some Juicers, the surgical operations are as fresh in their memory as the chemical cocktails flowing through their veins; “Last Call” is a long way off for this Juicer. \n\nShe adds +2 to her Burn Rating. This Edge must be taken at character creation, and Freshly Juiced characters reduce their number of Hero’s Journey rolls by one— they haven’t had much of a “journey” yet!" compset="Edge">
    <tag group="EdgeType" tag="Iconic"/>
    <tag group="MinRank" tag="0"/>
    <pickreq thing="grpSRAJuicer"/>
    </thing>
  <thing id="edgSRALLObsSphere" name="Ley Line Observation Sphere" description="Some Walkers are able to conjure up a glowing bluish-white sphere the size of a soccer ball, which they can send anywhere up or down a ley line. It travels at 30 mph and has a range of one mile, acting as a focus point for the Walker to use any of her senses to observe what’s around the ball as though she were standing there (she cannot cast her powers through it, however). Using the sphere counts as an action, imposing a −2 Multi-Action penalty on any other actions while active." compset="Edge">
    <tag group="EdgeType" tag="Iconic"/>
    <tag group="MinRank" tag="1"/>
    <pickreq thing="grpSRALeyLineWalk"/>
    </thing>
  <thing id="edgSRALLPhasing" name="Ley Line Phasing" description="Some Walkers become so intricately tied to the lines that they are capable of phasing into the energies for an instant, transporting to any other point along the line in the blink of an eye. \n\nLey Line Walkers with this Edge may take a full round to concentrate (taking no other actions, not even free actions), and on the next round can teleport flawlessly to any other point along the same line. Though it costs no PPE, the Walker must make a Vigor check or suffer a Fatigue level (requiring four hours to recover). Each subsequent use of this ability imposes a cumulative −2 penalty to the Vigor check until the Walker has a full night’s rest. Ley Line Phasing cannot be used to transport others, but see Ley Line Gate, below." compset="Edge">
    <tag group="EdgeType" tag="Iconic"/>
    <tag group="MinRank" tag="2"/>
    <pickreq thing="grpSRALeyLineWalk"/>
    </thing>
  <thing id="edgSRALLGate" name="Ley Line Gate" description="A truly powerful Ley Line Walker ability, the hero can carry up to his Smarts die in passengers when he phases to another location on the line. Carrying passengers, even just one, imposes an additional −2 penalty to his Vigor check to resist Fatigue." compset="Edge">
    <tag group="EdgeType" tag="Iconic"/>
    <tag group="MinRank" tag="3"/>
    <pickreq thing="edgSRALLPhasing"/>
    </thing>
  <thing id="edgSRAUpgradeable" name="Upgradeable" description="This hero can handle a lot more cybernetic invasion in his body than most. He increases his Maximum Strain by +6. Note Combat Cyborgs and Headhunters already have this Edge." compset="Edge">
    <usesource source="srcRiftsTLPGAE"/>
    <tag group="MinRank" tag="0"/>
    <tag group="EdgeType" tag="Background"/>
    <eval phase="Effects" priority="5500">#resmax[resStrainSRA] += 6</eval>
    </thing>
  <thing id="edgSRACharge" name="Charge" description="The warrior is experienced at moving across a large distance rapidly and getting the most effect out of rushing her foes. She may ignore the penalty for Running when making a Fighting attack at the end of her movement." compset="Edge">
    <usesource source="srcRiftsTLPGAE"/>
    <tag group="MinRank" tag="1"/>
    <tag group="EdgeType" tag="Combat"/>
    </thing>
  <thing id="edgSRAOverrun" name="Overrun" description="If he moves at least one inch beyond his normal Pace before making any Fighting attacks, the hero adds +2 to the damage rolls. Any target Shaken by such an attack is knocked prone." compset="Edge">
    <usesource source="srcRiftsTLPGAE"/>
    <tag group="MinRank" tag="1"/>
    <tag group="EdgeType" tag="Combat"/>
    <pickreq thing="edgSRACharge"/>
    </thing>
  <thing id="edgSRADirtyFightr" name="Dirty Fighter" description="This Edge is favored by those who fight to win at any cost. A fighter with this Edge gets a +2 when making Tests against a foe if his next action is a Fighting attack against that same target." compset="Edge">
    <usesource source="srcRiftsTLPGAE"/>
    <tag group="MinRank" tag="1"/>
    <tag group="EdgeType" tag="Combat"/>
    </thing>
  <thing id="edgSRATrickyFight" name="Tricky Fighter" description="The warrior is adept at feints and trickery in hand-to-hand combat. He ignores the Multi-Action penalty when making a Test against a foe when his next action is a Fighting attack against that target." compset="Edge">
    <usesource source="srcRiftsTLPGAE"/>
    <tag group="MinRank" tag="2"/>
    <tag group="EdgeType" tag="Combat"/>
    <pickreq thing="edgSRADirtyFightr"/>
    </thing>
  <thing id="edgSRABlaster" name="Blaster" description="Your hero has honed the lethality of her arcane attacks, unlocking a Power Modifier that further improves the damage of multiple powers.\n\n{b}MODIFIERS{/b}\n{indent -10} • BLASTER (+2): When using at least one Mega Power Modifier in conjunction with the blast, bolt, burst, or damage field powers increase the damage dice from d6 to d8. Armor Piercing grants an extra +1 AP." compset="Edge">
    <usesource source="srcRiftsTLPGAE"/>
    <tag group="MinRank" tag="2"/>
    <tag group="EdgeType" tag="Power"/>
    </thing>
  <thing id="edgSRAExpBlaster" name="Expert Blaster" description="Your blaster is an expert at improving the lethality of her arcane attacks.\n\n{b}MODIFIERS{/b}\n{indent -10} • EXPERT BLASTER (+3): When using at least one Mega Power Modifier in conjunction with the blast, bolt, burst, or damage field powers increase the damage dice from d6 to d10. Armor Piercing grants an extra +2 AP." compset="Edge">
    <usesource source="srcRiftsTLPGAE"/>
    <tag group="MinRank" tag="3"/>
    <tag group="EdgeType" tag="Power"/>
    <pickreq thing="edgSRABlaster"/>
    </thing>
  <thing id="edgSRAMasBlaster" name="Master Blaster" description="Your blaster is a master at creating the very deadliest manifestation of her arcane attacks.\n\n{b}MODIFIERS{/b}\n{indent -10} • MASTER BLASTER (+4):\nWhen using at least one Mega Power Modifier in conjunction with the blast, bolt, burst, or damage field powers increase the damage dice from d6 to d12. Armor Piercing grants an extra +3 AP." compset="Edge">
    <usesource source="srcRiftsTLPGAE"/>
    <tag group="MinRank" tag="3"/>
    <tag group="EdgeType" tag="Power"/>
    <pickreq thing="edgSRAExpBlaster"/>
    </thing>
  <thing id="edgSRACybPsyAlign" name="Cyber-Psychic Alignment" description="The character’s mind and body are aligned strongly enough to offset one level of Strain penalty when using powers. Having a single Strain’s worth of cybernetics means there is no impact on rolling for powers use, while having three points of Strain value embedded only applies a −2." compset="Edge">
    <usesource source="srcRiftsTLPGAE"/>
    <tag group="MinRank" tag="1"/>
    <tag group="EdgeType" tag="Background"/>
    <tag group="Arcane" tag="ReducedCybPen" name="Reduced Penalty for Cybernetics"/>
    <eval phase="Setup" priority="1000">perform hero.assign[Arcane.ReducedCybPen]</eval>
    </thing>
  <thing id="edgSRAImpCybPsyAl" name="Improved Cyber-Psychic Alignment" description="The character can ignore two levels of Strain penalties when using her powers." compset="Edge">
    <usesource source="srcRiftsTLPGAE"/>
    <tag group="MinRank" tag="1"/>
    <tag group="EdgeType" tag="Power"/>
    <pickreq thing="edgSRACybPsyAlign"/>
    </thing>
  <thing id="edgSRAMastMagic" name="Master of Magic" description="True understanding of the ley lines, Rifts, and eldritch flow of power through the world grants the spellcaster exceptional might and capability. The Master of Magic gains access to the Mega Power Modifiers for all known magic or miracles powers." compset="Edge" summary="Gain access to Mega Power modifiers for all known magic or miracles.">
    <usesource source="srcRiftsTLPGAE"/>
    <tag group="MinRank" tag="1"/>
    <tag group="EdgeType" tag="Power"/>
    </thing>
  <thing id="edgSRAPossession" name="Possession" description="This ability grants the Master Psionicist an additional Modifier to the puppet power. If three more ISP is spent when activating the power, he may inhabit and take control of a target mortal’s body. The power has its Duration increased to five minutes and requires a successful Psionics roll opposed by the target’s Spirit (empathy adds to this roll). \n\nWith a Critical Failure the caster may not attempt to possess the target again for 24 hours. With a success his mental essence inhabits the target’s mortal body, gaining complete control over the possessed person’s actions and physical abilities (including the host’s physical Attributes, Edges, and powers, but not Skills or memories). With a raise the victim is −2 to opposed rolls versus the possessor for the power’s Duration, which is increased to one hour base. The possessor may use psionic powers and abilities at a –2 while inhabiting a body of the same species, or –4 while inhabiting a similar but alien form. More alien forms could impose a higher penalty, or be impossible (at the GM’s discretion). \n\nThe host gets an opposed Spirit roll to break the possession whenever the possessor attempts to perform clearly harmful actions such as attacking the host’s friends or harming the host’s body. If successful the possessor loses his action, on a raise he is immediately ejected out of the host body and back into his own. \n\nDuring the period that the victim is possessed, the Master Psionic’s body is in a coma-like state and vulnerable to attack (including Finishing Moves if no allies are present); he may instantly return to his body at any time regardless of distance. If the host is Incapacitated the possessor is Stunned and returned to his body." compset="Edge">
    <usesource source="srcRiftsTLPGAE"/>
    <tag group="MinRank" tag="0"/>
    <tag group="EdgeType" tag="Background"/>
    </thing>
  <thing id="wpSRAPsiBlade" name="Psi-Blade" compset="Melee">
    <fieldval field="wpDamage" value="Str+Sma"/>
    <usesource source="srcRiftsTLPGAE"/>
    <tag group="Weapon" tag="MegaDamage"/>
    <tag group="Hide" tag="Equipment"/>
    <tag group="WeaponType" tag="Natural"/>
    </thing>
  <thing id="edgSRAPsiBlade" name="Psi-Blade" compset="Edge">
    <usesource source="srcRiftsTLPGAE"/>
    <tag group="MinRank" tag="1"/>
    <tag group="EdgeType" tag="Power"/>
    <bootstrap thing="wpSRAPsiBlade"></bootstrap>
    <eval phase="Render" priority="5500"><![CDATA[~build damage string
var dmg as string
var strength as string
var smart as string

strength = hero.child[attrStr].field[trtDisplay].text
smart = hero.child[attrSma].field[trtDisplay].text

dmg = strength & "+" & smart

~calculate the AP for the Psi-Blade
var AP as number

AP = hero.childfound[skPsionics].field[trtFinal].value * 2

~apply to the Psi-Blade
perform hero.childfound[wpSRAPsiBlade].setfocus
focus.field[wpDamage].text = dmg
focus.field[wpPiercing].value += AP]]>
      <after name="Calc trtDisplay"/>
      </eval>
    </thing>
  <thing id="edgSRAExcRapRech" name="Exceptional Rapid Recharge" description="The character now regains 30 Power Points (PPE/ISP) per hour spent resting." compset="Edge">
    <usesource source="srcRiftsTLPGAE"/>
    <tag group="MinRank" tag="2"/>
    <tag group="EdgeType" tag="Power"/>
    <pickreq thing="edgRapRch2SWADE"/>
    </thing>
  <thing id="edgSRASixthSense" name="Sixth Sense" description="A psionic can develop extraordinary extrasensory perception and awareness of potential danger. When rolling for Surprise, the character gains the normal Danger Sense bonus, and with a success begins the round on Hold. With a raise, she can take a single immediate action (such as run to cover, activate a defensive power, or draw a weapon). \n\nSixth sense eliminates the −2 to Notice checks in other situations covered by Danger Sense." compset="Edge">
    <usesource source="srcRiftsTLPGAE"/>
    <tag group="MinRank" tag="2"/>
    <tag group="EdgeType" tag="Power"/>
    </thing>
  <thing id="edgSRATelemech" name="Telemechanics" description="This peculiar yet powerful ability allows the Master Psionic to directly interface with machines, including vehicles, weapons, computers, electronic devices, and robots. It reveals complete, expert knowledge of how the machine operates; a schematic diagram and perfect operational readouts are seen in the mind’s eye. \n\nWhen activating mind link the psionic can telepathically connect to a machine instead of an individual. They gain a +2 to Common Knowledge, Electronics, Hacking, Research, Repair, and Thievery rolls related to the target machine; this includes attempts to dig through data files or bypass security systems and electronic locks. \n\nThough it can boost the hero’s understanding of machines, Telemechanics does not allow the Master Psionic to telepathically control intelligent machines, cybernetic/bionic systems, vehicles, or weapons. \n\nTelemechanics is completely ineffective on arcane items or constructions (including Techno-Wizardry devices)." compset="Edge">
    <usesource source="srcRiftsTLPGAE"/>
    <tag group="MinRank" tag="0"/>
    <tag group="EdgeType" tag="Background"/>
    </thing>
  <thing id="edgSRACombatAce" name="Combat Ace" description="With this Edge, the hero ignores the Multi-Action penalty for making a Boating, Driving, or Piloting check and taking another action in the same round." compset="Edge">
    <usesource source="srcRiftsTLPGAE"/>
    <tag group="MinRank" tag="1"/>
    <tag group="EdgeType" tag="Profession"/>
    <pickreq thing="edgAceSWADE"/>
    </thing>
  <thing id="edgSRAPAJock" name="Power Armor Jock" description="Power armor requires a basic level of training and experience to operate effectively. A character must have this Edge to avoid a −2 penalty to Agility and all skill rolls while wearing power armor." compset="Edge">
    <fieldval field="shortname" value="PA Jock"/>
    <usesource source="srcRiftsTLPGAE"/>
    <tag group="MinRank" tag="0"/>
    <tag group="EdgeType" tag="Profession"/>
    <pickreq thing="edgAceSWADE"/>
    </thing>
  <thing id="edgSRARVJock" name="Robot Vehicle Jock" description="Robot vehicles are highly complex war machines which require a great deal of training to operate. Without this Edge, the operator suffers a −2 penalty to all skill rolls while piloting a robot vehicle." compset="Edge">
    <fieldval field="shortname" value="RV Jock"/>
    <usesource source="srcRiftsTLPGAE"/>
    <tag group="MinRank" tag="0"/>
    <tag group="EdgeType" tag="Profession"/>
    <pickreq thing="edgAceSWADE"/>
    </thing>
  <thing id="edgSRAIKnowAGuy" name="I Know A Guy" description="Some people seem to know someone everywhere they go or make contacts quickly and easily. The contact may not be the most amazingly helpful person—they may even have an unrequited crush on the hero or hate them for some past slight. The person known may be well-placed in an organization, or just as easily a homeless person who knows the word on the street. The point is, however, almost anywhere he goes, the hero seems to have a knack for connecting with people who know something of value or can contact some helpful folks.\n\nOnce per session, the player can invoke this Edge to effectively have the Connections Edge with any person or group who shares a mutual interest or motivations (whether personally or as a group). Make a Smarts roll at −2 (or an opposed Smarts roll for powerful contacts) to see if they can be leveraged or convinced to help in any way. A failure on the check expends that session’s use of the Edge. If successful, the favor depends on the nature of the contact (GM’s call) but might include loaning useful gear, a few allied fighters, transportation, information, or even a professional with critical skills the party doesn’t have, like a hacker, occultist, or scholar." compset="Edge">
    <usesource source="srcRiftsTLPGAE"/>
    <tag group="MinRank" tag="0"/>
    <tag group="EdgeType" tag="Social"/>
