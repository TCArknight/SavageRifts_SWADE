    <tag group="User" tag="UserSelect"/>
    </thing>
  <thing id="eqSRABioAnalysisK" name="Bio-Analysis Kit" description="Specifically designed to handle situations where biological systems are compromised via disease, toxins, and related conditions, this tackle-box-sized kit contains a miniature bio-analysis machine and a miniature “lab” that can be used to quickly concoct antidotes and treatments. It requires a Healing skill of at least d4 to use. Anyone with Science gains a +1 using this system, or a +2 if they also have Electronics." compset="Equipment" summary="Requires a Healing skill of at least d4 to use. Anyone with Science gains a +1 using this system, or a +2 if they also have Electronics.">
    <fieldval field="gearWeight" value="5"/>
    <fieldval field="grCost" value="7200"/>
    <tag group="GearType" tag="Adventure"/>
    </thing>
  <thing id="eqSRASepAdventure" name="– Adventuring Gear –" compset="Equipment" isshowonly="yes">
    <usesource source="srcRiftsTLPGAE"/>
    <tag group="GearType" tag="Adventure" name="Adventuring Gear" abbrev="Adventuring Gear"/>
    </thing>
  <thing id="edgSRALinguist" name="Linguist" description="This world-traveler has an ear for languages. She begins play knowing half her Smarts die type in different Language skills of her choice at d6 (see the Language skill on page 31)." compset="Edge" summary="Hero knows twice as many languages" replaces="edgLingist" uniqueness="unique">
    <usesource source="srcRiftsTLPGAE"/>
    <tag group="EdgeType" tag="Background" name="Background" abbrev="Background"/>
    <tag group="MinRank" tag="0" name="Novice" abbrev="Novice"/>
    <eval phase="Final" priority="2000">perform hero.assign[Hero.ShowLang]
      #resmax[resLang] += (#trait[attrSma] / 2)
      #resleft[resLang] += (#trait[attrSma] / 2)
      if (hero.tagis[Hero.SmartsLang] = 0) then
        #resmax[resLang] += (#trait[attrSma] / 2)
        #resleft[resLang] += (#trait[attrSma] / 2)
        endif</eval>
    <eval phase="PreTraits" priority="5000" index="2">perform hero.assign[Hero.Linguist]</eval>
    <exprreq message="Smarts d6 required."><![CDATA[#trait[attrSma] >= 3]]></exprreq>
    </thing>
  <thing id="edgSRASepRacial" name="– Racial Edges –" compset="Edge" isshowonly="yes">
    <usesource source="srcRiftsTLPGAE"/>
    <tag group="EdgeType" tag="Racial"/>
    </thing>
  <thing id="siSRABreedAgile" name="Agile" compset="Simple">
