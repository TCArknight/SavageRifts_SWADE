    <pickreq thing="edgAceSWADE"/>
    </thing>
  <thing id="edgSRARVJock" name="Robot Vehicle Jock" description="Robot vehicles are highly complex war machines which require a great deal of training to operate. Without this Edge, the operator suffers a −2 penalty to all skill rolls while piloting a robot vehicle." compset="Edge">
    <fieldval field="shortname" value="RV Jock"/>
    <usesource source="srcRiftsTLPGAE"/>
