    <eval phase="Effects" priority="5000">perform hero.assign[HelperRiftsAE.PPENotValid]</eval>
    </thing>
  <thing id="rpSRAWantedMiMFB" name="Wanted (Minor)" description="Mutant animals refusing to return to Coalition society are wanted by the CS and deemed feral. This threat is backed by a standing bounty for their capture." compset="RacialProp">
    <usesource source="srcRiftsEmpHumAE"/>
    <tag group="Hide" tag="Special"/>
    <bootstrap thing="hinWanted">
      <assignval field="domDomain" value="Coalition"/>
      </bootstrap>
    </thing>
  <thing id="rpSRAWeakLLMFB" name="Weakness (Ley Line Hypersensitivity)" description="Dog Boys lose their Psychic Sense on or close (within 2&quot; or 4 yards) to a ley line. Ley line storms cause a level of Fatigue from severe headaches and distress as for a psychic (which does not stack if they have the Arcane Background)." compset="RacialProp">
    <usesource source="srcRiftsEmpHumAE"/>
    </thing>
  <thing id="rpSRABiteClawMFK" name="Bite/Claws" description="Battle Cats have claws and teeth dealing Str+d4 damage, see {b}Natural Weapons{/b} in {i}Savage Worlds{/i}." compset="RacialProp" summary="Bite &amp; Claws doing Str+d4 damage.">
    <usesource source="srcRiftsEmpHumAE"/>
    <bootstrap thing="wpSRAClaws">
      <autotag group="WeaponDie" tag="2"/>
      </bootstrap>
    <bootstrap thing="wpSRABite">
      <autotag group="WeaponDie" tag="2"/>
      </bootstrap>
    </thing>
  <thing id="rpSRABreedAdvMFK" name="Breed Advantage" description="As with Dog Boys, various species of cats provide distinct genetic advantages that can express over time, see {i}The Tomorrow Legion Player’s Guide{/i} for details." compset="RacialProp">
    <usesource source="srcRiftsEmpHumAE"/>
    <tag group="Hide" tag="Special"/>
    <eval phase="PreTraits" priority="5000">~set resRaceEdge1 to selection
#resmax[resRaceEdge1] += 1</eval>
    <eval phase="Render" priority="10000" index="2"><![CDATA[var header as string
var chooser as string
var additem as string

additem = "Add Breed Advantage"
header = "Breed Advantage: "
header &= hero.child[resRaceEdge1].field[resSummary].text
chooser = "Helper.BreedAdvMain"

~set up chooser expressions for Breed Advantages
perform hero.child[RacialHelper1].setfocus
focus.field[rhEdgeAdd].text = additem
focus.field[rhEdgeExp].text = chooser
focus.field[rhEdgeHdr].text = header]]></eval>
    <eval phase="Setup" priority="500" index="3">perform hero.assign[Helper.BreedValid]</eval>
    </thing>
  <thing id="rpSRACuriousMFK" name="Curious" description="Every mutant feline has the Curious Hindrance which is part of what makes them so great at recon and search missions, though it may raise their mortality rate." compset="RacialProp" summary="Begin with Curious Hindrance">
    <usesource source="srcRiftsArcMysAE"/>
    <tag group="Hide" tag="Special"/>
    <bootstrap thing="hinCurious"></bootstrap>
    </thing>
  <thing id="rpSRAKeenHearMFK" name="Keen Sense of Hearing" description="Mutant felines have incredibly powerful hearing and can directionally home in on sound emanations and discern minute differences. They gain a +2 bonus on hearing-based Notice checks." compset="RacialProp" summary="+2 bonus on hearing-based Notice checks.">
    <usesource source="srcRiftsEmpHumAE"/>
    <tag group="User" tag="Activation"/>
    <eval phase="PreTraits" priority="5000"><![CDATA[if (field[abilActive].value <> 0) then  
  perform #traitroll[skNotice,+,2,"Keen Hearing"]  
  endif]]></eval>
    </thing>
  <thing id="rpSRALeaperMFK" name="Leaper" description="Mutant felines can jump twice as far as normal and add +4 to damage when making a Wild Attack when coupled with a leap, instead of the usual +2 (needs room to leap)." compset="RacialProp">
    <usesource source="srcRiftsEmpHumAE"/>
    </thing>
  <thing id="rpSRALowLightMFK" name="Low Light Vision" description="Ignore Dim or Dark Illumination penalties." compset="RacialProp" summary="No penalties for Dim/Dark lighting" uniqueness="unique">
